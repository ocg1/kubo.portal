<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
			     xmlns:p  = "http://primefaces.org/ui">
			     
	<div class="dvContent" style = "float: left; width: 180px">
		<div class="dvLabel stlNumExt" style = "margin: 0;">
			<h:outputLabel value = "#{msg.ExtNumber_text}" styleClass = "editor-domicilio label"/>
		</div>
		
		<div class="dvElement" style = "width: 120px; padding: 0">
			<h:inputText id   = "edicion_vivienda_address_number" 
						 size = "4" 
						 
						 value = "#{summaryRequest.editor_domicilio.address.address_number}" 
						 styleClass = "elementAddresMin mapClass address_number editor-domicilio input-text">
				<p:ajax event = "blur"  listener = "#{summaryRequest.editor_domicilio.listener_generar_vivienda_TOKEN}"/>
			</h:inputText>
			
		</div>
	</div>			     
</ui:composition>	