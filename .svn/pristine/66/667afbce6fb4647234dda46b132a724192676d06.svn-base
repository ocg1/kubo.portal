package mx.com.kubo.model;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="gn_tutor")
public class Tutor implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	@Id
	private int tutor_id;
	@Column
	private Integer company_id;
	@Column
	private Integer prospectus_id;
	@Column
	private Integer prospectus_id_tutor;
	public int getTutor_id() {
		return tutor_id;
	}
	public void setTutor_id(int tutor_id) {
		this.tutor_id = tutor_id;
	}
	public Integer getCompany_id() {
		return company_id;
	}
	public void setCompany_id(Integer company_id) {
		this.company_id = company_id;
	}
	public Integer getProspectus_id() {
		return prospectus_id;
	}
	public void setProspectus_id(Integer prospectus_id) {
		this.prospectus_id = prospectus_id;
	}
	public Integer getProspectus_id_tutor() {
		return prospectus_id_tutor;
	}
	public void setProspectus_id_tutor(Integer prospectus_id_tutor) {
		this.prospectus_id_tutor = prospectus_id_tutor;
	} 
	
}
