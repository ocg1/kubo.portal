<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">

	<div class="centerPosition">
		<div  id = "detailsOthers" class = "details4">
		
			<p style="width: 60px; float: left;">
				<strong>Otros</strong>
			</p>
			
			<ui:include src = "otros/bitacora-cambios.xhtml"/>
			
				<div style = "display: none">
					<h:commandLink id = "update-section-phone" immediate = "true">
						<p:ajax event = "click" update = "updateSectionPhone show-ischange-phone"/>
					</h:commandLink>					
				</div>	
																	
			<ul>
				<h:panelGroup id="updateSectionPhone">
								
					<li style="margin-bottom: 20px;">
						<p>
							Proporcionó 
							<h:outputLabel value="#{summaryRequest.telefono.listPhone.size()}" />
							<h:outputLabel value="#{summaryRequest.telefono.listPhone.size() > 1 ? 'teléfonos' : 'teléfono'}" />
						</p>		
									
						<ui:include src = "tooltip/lista_de_telefonos.xhtml"/>
						<ui:include src = "otros/lista_de_telefonos.xhtml"/>
						<ui:include src = "otros/agregar-telefono.xhtml"/>																																								
					</li>								
				</h:panelGroup>				
								
				<h:outputText escape="false" value="#{summaryRequest.computerHome}"/>
				<h:outputText escape="false" value="#{summaryRequest.computerBusiness}"/>
				<h:outputText escape="false" value="#{summaryRequest.computerEmployment}"/>
				<h:outputText escape="false" value="#{summaryRequest.internetBusiness}"/>
				<h:outputText escape="false" value="#{summaryRequest.internetEmployment}"/>
				<h:outputText escape="false" value="#{summaryRequest.internetHome}"/>
				<h:outputText escape="false" value="#{summaryRequest.printer}"/>
				
			</ul>
		</div>	
	</div>
	
	<script>
	  console.log( "otros" );
	</script>
						
</ui:composition>				