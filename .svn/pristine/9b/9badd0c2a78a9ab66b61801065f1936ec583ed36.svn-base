<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">	

	<h:form styleClass="areaL" id = "frm_beneficiaries" prependId = "false" >
	
		<div id = "general">
			<div class = "backStyle">	
			
				<ui:include src = "paso_3_beneficiario/beneficiario/cuenta_inversion.xhtml"/>
								
				<p style="clear: both;"/>
	
				<div id = "contFrmPre">
