					<ui:include src = "paso_3_beneficiario/beneficiario/porcentaje.xhtml"/>
									
					<div class = "dvContent" style = "margin-left: auto; margin-right: auto;">
						<div class = "solicitud callAction btnNaranja" id = "nextInvBen" onclick="validationBtn('Beneficiarios');">
