<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
				
	<h:inputText    id = "numphone" maxlength = "13"
				 value = "#{p.numPhone}"  
			onkeypress = "return isNumberKey(event);"  
			styleClass = "elementAddres1" style = " width: 361px !important; height: 30px !important;">
					
		<p:ajax event = "blur" 
				onstart  = "return fieldNotEmpty('uiphone\:#{p.index}\:numphone');"
				oncomplete = "Telefonos.phone_number_oncomplete(xhr, status, args);"
				listener   = "#{summaryRequest.telefono.init_phone_number}" />
				
	</h:inputText>					
</ui:composition>				