<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">			
					
	<style>
		#tipo_nota_SOM select
		{
			font-size: 1em ! important; 
			height: 28px ! important; 
			background: none repeat scroll 0% 0% transparent ! important; 
			border: 0px none ! important; 
			box-shadow: 0px 0px 0px 0px rgb(194, 194, 194) ! important; 
			width: 415px ! important;
		}
	</style>
	
	<h:outputLabel value = "Tipo de nota" />

	<div id = "tipo_nota_SOM" class = "styled-select">																			
		<h:selectOneMenu value  = "#{summaryRequest.notas.tipo_nota_SELECTED}">									 
			<f:selectItem itemValue = "#{summaryRequest.notas.tipo_nota_SELECTED}"  
						  itemLabel = "#{summaryRequest.notas.tipo_nota_name}"/>								   																																		
		</h:selectOneMenu>																							
	</div>	
</ui:composition>	