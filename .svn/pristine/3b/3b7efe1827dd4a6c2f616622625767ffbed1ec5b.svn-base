<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
	<li>
		<label class="etiqueta"> Mi Cuenta Clabe:</label>
	
			<div class="campo">
				<div class="styled-select select_border">
					<h:selectOneMenu id = "cuentasUsrDeposito" 
								     tabindex   = "1" 
								     styleClass = "elementStlSelAddress" 
								     value      = "#{disposicionEfectivo.clabeSel}" 
								     immediate  = "true">  
				        
				        <f:selectItem itemValue = "" itemLabel = "Seleccione una Clabe Interbancaria" />
				        
				        <f:selectItems value = "#{disposicionEfectivo.lstClabes}" 
				        			   var   = "account" 
				        			   itemLabel = "#{account.mx_clabe}" 
				        			   itemValue = "#{account.mx_clabe}"/>
				        			   
				        <p:ajax event = "change"  />
				          
				    </h:selectOneMenu>
				</div>
			</div>
	
	</li>				
</ui:composition>				