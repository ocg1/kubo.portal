<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
				 xmlns:f  = "http://java.sun.com/jsf/core"
				 xmlns:p  = "http://primefaces.org/ui">	
						
	<h:panelGroup id = "section-data-ife" rendered = "#{sessionBean.area == 'M'}">					
		<div class = "details4" style = "float:left">
			<p >
				<strong>Datos de identificación</strong>
			</p>
			
			<h:panelGroup rendered = "#{!summaryRequest.editor_ife_ENABLED and summaryRequest.mx_ine_cic != null}">
				<table class="formato_tablas" cellspacing="0" cellpadding="0">
				<tr>
					<th>
						Tipo de identificación:
					</th>
					
					<td>INE</td>										
				</tr>
				
				<tr>
					<td>Número CIC:</td>
					<td>#{summaryRequest.mx_ine_cic}</td>
				</tr>
				</table>				
			</h:panelGroup>
			
			<h:panelGroup rendered = "#{summaryRequest.editor_ife_ENABLED}">
				<ui:include src = "perfil/editor-IFE/bitacora.xhtml" />		
				
				<table class="formato_tablas" cellspacing="0" cellpadding="0">
					<thead>
						<tr>
							<th >
								Tipo de identificación:
							</th>
							
							<th>IFE</th>
						</tr>
					</thead>
					
					<tbody>
						<ui:repeat id = "uiife" 
								   var   = "changeife" varStatus = "status"
								   value = "#{summaryRequest.lstChangeIFE}">
					
							<ui:include src = "perfil/editor-IFE.xhtml" />																					
						</ui:repeat>																	
					</tbody>																
				</table>
			</h:panelGroup>
		</div>
	</h:panelGroup>
	
	<script>
	  console.log( "datos_identificacion.xhtml" );
	</script>
	
</ui:composition>				