<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:c  = "http://java.sun.com/jsp/jstl/core"
				xmlns:p  = "http://primefaces.org/ui">
				
	<div>
		 <h:panelGroup rendered="#{summaryRequest.displayTransUnion}">
		 	<p:outputPanel rendered="#{summaryRequest.actualProyect.transunion!=null }"  >
				
				 <h:panelGroup rendered="#{summaryRequest.actualProyect.transunion!=null}">
					 <p id="detailsTransUnion" class="#{summaryRequest.validTransUnion?'importantCls':'smallGray'}">
					 	T Score: #{summaryRequest.actualProyect.transunion==null?'No Definido':summaryRequest.actualProyect.transunion.transunion_bc_score==null?'Sin Score':summaryRequest.actualProyect.transunion.transunion_bc_score} 
					 </p>
				 </h:panelGroup>
				 
				 <h:panelGroup rendered="#{summaryRequest.actualProyect.transunion==null}">
					 <p class="smallGray">
					 	T Score: No Definido 222
					 </p>
				 </h:panelGroup>
				 
				 <div id = "dvDetalleTransUnion" >
										 	
				 	<div style="position: absolute;top: -18px;right: -6px; width: 30px; height: 30px; z-index: 180;" >
	          			<p:graphicImage url="../resources/img/close.png"  style="cursor: pointer;" onclick="hideTransUnion();" ></p:graphicImage>
	          		</div>
	          		
				 	BC Score inicial: #{summaryRequest.actualProyect.bc_score}<br />
				 	
				 	<h:panelGroup rendered="#{summaryRequest.actualProyect.transunion!=null}">
					 	<div style="height: auto; overflow: auto;margin-top: 4px;margin-bottom: 1px;">
					 		Incremento 1: #{summaryRequest.actualProyect.transunion.transunion_mod1}
					 	</div>
				 	</h:panelGroup>
				 	
				 	<h:panelGroup rendered="#{summaryRequest.actualProyect.transunion!=null and summaryRequest.actualProyect.transunion.ajuste1!=null}">
					 	<div style="height: auto; overflow: auto;margin-top: 1px;margin-bottom: 6px;color:#6e6e6e;">
					 		#{summaryRequest.actualProyect.transunion.ajuste1.description}
					 	</div>
				 	</h:panelGroup>
				 	
				 	<h:panelGroup rendered="#{summaryRequest.actualProyect.transunion!=null}">
					 	<div style="height: auto; overflow: auto;margin-top: 4px;margin-bottom: 1px;">
					 		Incremento 2: #{summaryRequest.actualProyect.transunion.transunion_mod2}
					 	</div>
				 	</h:panelGroup>
										 	
				 	<h:panelGroup rendered="#{summaryRequest.actualProyect.transunion!=null and summaryRequest.actualProyect.transunion.ajuste2!=null}">
					 	<div style="height: auto; overflow: auto;margin-top: 1px;margin-bottom: 6px;color:#6e6e6e;">
					 		#{summaryRequest.actualProyect.transunion.ajuste2.description}
					 	</div>
				 	</h:panelGroup>
				 	
				 	<h:panelGroup rendered="#{summaryRequest.actualProyect.transunion!=null}">
					 	<div style="height: auto; overflow: auto;margin-top: 4px;margin-bottom: 4px;">
					 		BC Score final: #{summaryRequest.actualProyect.transunion.transunion_bc_score}
					 	</div>
				 	</h:panelGroup>
				 	
				 	<h:panelGroup rendered="#{summaryRequest.transErrorMsg!=null}">
					 	<div style="height: auto; overflow: auto;margin-top: 4px;color: #FF0000;margin-bottom: 4px;">
					 		#{summaryRequest.transErrorMsg}
					 	</div>
				 	</h:panelGroup>
				 </div>				 
			 </p:outputPanel>
									 
			 <p:outputPanel rendered = "#{summaryRequest.actualProyect.transunion==null}"  >
				 <p id = "detailsTransUnionNotDef" class="smallGray">
				 	TransUnion: No Definido
				 </p>
			 </p:outputPanel>
			 
		 </h:panelGroup>
		 
	<!--  <p id="detailsPublishedDate" class="smallGray">#{sessionBean.area=='M'?'BC Score':'Score'}: #{summaryRequest.score.bc_score} 
			<h:panelGroup rendered="#{summaryRequest.displayReport}"> 
				<a href="reporteBuro.jsf?prospecto=" target="_blank">
					<img style="width: 25px; height: 25px;" alt="Reporte" src="../resources/img/chart-32.png" />
				</a>
			</h:panelGroup> 
		 </p> -->
		 
	<!-- <p id="detailsPublishedDate" class="smallGray">ICC:   #{summaryRequest.score.cci_score}</p> -->
	</div>				
</ui:composition>				