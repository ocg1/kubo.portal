@CHARSET "UTF-8";

.custumStlFileUpload.ui-fileupload,
.custumStlFileUploadPreview.ui-fileupload
{
	font-size: 0.7em !important;	
}
.custumStlFileUpload .ui-fileupload .fileupload-buttonbar,
.custumStlFileUploadPreview .ui-fileupload .fileupload-buttonbar
{
	padding: 0 !important;
}

.custumStlFileUpload, 
.ui-widget-header,
.custumStlFileUploadPreview
{	
	background-color: #FFF;
	background-image: none;
    border: none;
    color: #333333 !important;
    font-weight: bold;
    text-shadow: 0 1px 0 rgba(255, 255, 255, 0.698);
    padding-left: 0 !important;
}

.custumStlFileUpload .fileupload-buttonbar .ui-button input,
.custumStlFileUploadPreview .fileupload-buttonbar .ui-button input
{
	height: 30px !important;
	right: 80px !important;
	top: -7px !important;
	width: 175px !important;
}

.fileupload-content{
	border-top-width: 0;
    padding: 0.2em 0.4em;
}

.ui-corner-bottom{
	-moz-border-radius-bottomleft: 3px;
    -moz-border-radius-bottomright: 3px;
	display: none;
}

.ui-widget-content
{
	background: none repeat scroll 0 0 #FFFFFF;
    border: none ;
    color: #4F4F4F;
}

.custumStlFileUpload .fileupload-buttonbar .ui-button,
.custumStlFileUploadPreview .fileupload-buttonbar .ui-button,
.custumStlFileUploadPreview .start .ui-button,
.custumStlFileUploadPreview .cancel .ui-button{
   	border: dashed 2px #ccc !important;
   	width: 62% !important;
   	display: block;
   	position:relative;
   	-webkit-appearance:none;
   	color:none;
   	background:none;
   	padding: 42px 3%;
   	font-size: 16px;
   	text-transform: none;
   	color: #999;
   	font-weight: normal;
   	text-align: center;
}




.custumStlFileUploadPreview .start .ui-button,.custumStlFileUploadPreview .cancel .ui-button{
	width: 90px !important;
	height: auto;
	font-weight: bold !important;
	display: block;
	margin: 0;
}
.custumStlFileUploadPreview .start .ui-button:before{
	content: "Subir"
}
.custumStlFileUploadPreview .cancel .ui-button:before{
	content: "Cancelar"
}
.custumStlFileUploadPreview .files td{
	padding-right: 5px !important;
	vertical-align: top;
}

.custumStlFileUploadPreview .files td.preview{
	padding-left: 0 !important;
}
.custumStlFileUpload .fileupload-buttonbar .ui-button:hover,
.custumStlFileUploadPreview .fileupload-buttonbar .ui-button:hover,
.custumStlFileUploadPreview .start .ui-button:hover,
.custumStlFileUploadPreview .cancel .ui-button:hover{
		color:#C1D82F ;
		box-shadow:none;
		border:dashed 2px #C1D82F !important;
}

.custumStlFileUpload .ui-state-default, .ui-widget-content .ui-state-default, .ui-widget-header .ui-state-default,.custumStlFileUploadPreview .ui-state-default{
	text-shadow: none !important;
}

.custumStlFileUpload .fileupload-content .ui-progressbar{
	height: 8px !important;
    width: 400px;
}
.custumStlFileUploadPreview .fileupload-content .ui-progressbar{
	height: 15px !important;
    width: 150px;
    border: 1px solid #333 !important;
}
.custumStlFileUpload .custumStlFileUpload .files,.custumStlFileUploadPreview .custumStlFileUpload .files{
    border-collapse: collapse;
    margin: 0 !important;
}

.custumStlFileUpload .ui-state-default .ui-icon,.custumStlFileUploadPreview .ui-state-default .ui-icon{
	background-image: none !important;	
}

.custumStlFileUpload .ui-button-text,.custumStlFileUploadPreview .ui-button-text{
	padding: 0;
	/* display: none; */
}
.custumStlFileUpload .ui-state-active,.custumStlFileUploadPreview .ui-state-active{
	color: inherit !important ;
}

.custumStlFileUpload td.preview,.custumStlFileUpload td.name,.custumStlFileUpload td.size,.custumStlFileUpload td.start,.custumStlFileUpload td.cancel,.custumStlFileUploadPreview td.name,.custumStlFileUploadPreview td.size{
	display: none !important;
}

