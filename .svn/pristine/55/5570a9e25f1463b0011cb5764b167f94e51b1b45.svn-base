<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"				 
				 xmlns:p  = "http://primefaces.org/ui">
				 	
	<div id = "tus-contratos" class = "notificacionListado" style = "display: none;">
		<ol>
			<li class="encFechaNotificacion">Fecha</li>
			<li class="encDescripcionNotificacion">Descripción</li>
		</ol>
		
		<ui:repeat value = "#{notificaciones_sistema.contratos}" var = "file">
			<ul>
				<li class="FechaNotificacion" ><strong><sub class="caption">Fecha</sub>#{file.upload_date}</strong></li>
				
				<li class="DescripcionNotificacion" ><sub class="caption">Descripción</sub>
					<a href="../templates/showpicture.xhtml?url=#{file.location}" target="_blank">#{file.fileType.name}</a>
				</li>
			</ul>
		</ui:repeat>
	</div>			 
</ui:composition>				 