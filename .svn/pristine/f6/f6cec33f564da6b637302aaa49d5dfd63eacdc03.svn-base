<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:p  = "http://primefaces.org/ui">
							
	<div id = "remote-domicilio-actividad" style = "display: none">	 		   		 		   
		<h:commandLink id = "init-lista-coincidencias-NIVEL-1" immediate = "true">
			<p:ajax event = "click"  update = "panel-lista-coincidencias-actividad"
					onstart    = "displayMessageProcessing('msgprocessing',false);"
		 		    oncomplete = "listener_lista_coincidencias(xhr, status, args);"
					listener   = "#{summaryRequest.domicilio_actividad.listener_lista_coincidencias}"/>
		</h:commandLink>		
		
		<h:commandLink id = "init-lista-coincidencias-NIVEL-2" immediate = "true">
			<p:ajax event = "click"  update = "panel-lista-coincidencias-actividad"
					onstart    = "displayMessageProcessing('msgprocessing',false);"
		 		    oncomplete = "listener_lista_coincidencias(xhr, status, args);"
					listener   = "#{summaryRequest.domicilio_actividad.listener_lista_coincidencias_NIVEL_2}"/>
		</h:commandLink>	 
		
		<h:commandLink class = "init-lista-neighborhood" immediate = "true">
			<p:ajax event = "click" update = "activity-neighborhood-id"
					oncomplete = "ActividadDomicilio.lista_neighborhood_oncomplete(xhr, status, args);"
					listener = "#{summaryRequest.domicilio_actividad.init_lista_neighborhood}"/>
		</h:commandLink>		
		
		<h:commandLink class = "init-domicilio-actividad" immediate = "true">
			<p:ajax event = "click" update = "panel-actividad-economica vivienda"
					oncomplete = "ActividadDomicilio.domicilio_actividad_oncomplete(xhr, status, args);"
					listener = "#{summaryRequest.init_domicilio_actividad}"/>
		</h:commandLink>	   
	</div>						
</ui:composition>				