<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:c  = "http://java.sun.com/jsp/jstl/core"
				xmlns:p  = "http://primefaces.org/ui">
	 <div class="statusBitacora" align = "right" 
	 	  style = "width: 30px; 
	 	  		   float: right;">
			<p:commandLink immediate      = "true" 
						   rendered       = "#{summaryRequest.changeStatusProyect!=null and summaryRequest.changeStatusProyect.hasChange}" 				  			   
			  			   actionListener = "#{summaryRequest.setListChanges(summaryRequest.changeStatusProyect)}" 
			  			   oncomplete     = "showPopUpChanges();" 
			  			   style          = "cursor:pointer;">
			  			   
				<h:graphicImage url      = "../resources/img/change-icon.png" 
							    rendered = "#{summaryRequest.changeStatusProyect != null and summaryRequest.changeStatusProyect.hasChange}"
								styleClass = "change_icon"/>
			</p:commandLink>
	</div>				
</ui:composition>				