<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
			     xmlns:p  = "http://primefaces.org/ui">
			     
	<div class="dvContent" style = "float: left; width: 180px">
		<div class="dvLabel stlNumExt" style = "margin: 0;">
			<h:outputLabel value = "#{msg.ExtNumber_text}" styleClass = "editor-domicilio label"/>
		</div>
		
		<div class="dvElement" style = "width: 120px; padding: 0">
			<h:inputText id = "actividad-address-number" size = "4" immediate = "true"
						 value = "#{summaryRequest.domicilio_actividad.address.address_number}" 
						 styleClass = "elementAddresMin mapClass address_number editor-domicilio input-text">
						 
				<p:ajax event = "change"
						oncomplete = "ActividadDomicilio.address_field_oncomplete(xhr, status, args);"  
						listener = "#{summaryRequest.domicilio_actividad.init_address_field}"/>
			</h:inputText>
			
		</div>
	</div>
	
	<div class="dvContent" style="float: left; width: 135px;">
		<div class="dvLabel stlNumInt" style = "margin: 0;">
			<h:outputLabel value = "#{msg.IntNumber_text}" styleClass = "editor-domicilio label"/>
		</div>
		
		<div class="dvElement" style="width: 120px; float: left; padding: 0">
			<h:inputText id = "actividad-apt-number" size = "4" immediate = "true"
					     value = "#{summaryRequest.domicilio_actividad.address.apt_number}" 
					     styleClass = "elementAddresMin editor-domicilio input-text">
					     
				<p:ajax event = "change"
						oncomplete = "ActividadDomicilio.address_field_oncomplete(xhr, status, args);" 
						listener = "#{summaryRequest.domicilio_actividad.init_address_field}"/>
			</h:inputText>
		</div>
	</div>	
	
</ui:composition>			     