<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
			     xmlns:p  = "http://primefaces.org/ui">
			     
	<h:panelGroup rendered = "#{sessionBean.coachProspectus_id != null}" >	   
		<div class = "dvContent" >
		
			<div class = "dvLabel" style = "width: 120px; clear: both;">
				<h:outputLabel styleClass = "labelsStl" value="#{msg.BetweenStreet2_text}" />
			</div>
			
			<p />
			
			<div class = "dvElementAddres1">
				<h:inputText immediate = "true"
							 onfocus = "Domicilio.init_focus_date(1);" 
						     value = "#{basicData.domicilio.address.second_street_reference}"  
						     styleClass = "elementAddres1" >
						     
					<p:ajax event = "blur" 
							oncomplete = "Domicilio.second_reference_on_complete(xhr, status, args);"
							listener   = "#{basicData.domicilio.init_second_reference}"/>
				</h:inputText>
			</div>
		</div>
	</h:panelGroup>
</ui:composition>	