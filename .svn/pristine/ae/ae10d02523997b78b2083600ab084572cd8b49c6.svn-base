div.partner-info.input
{
	float: left; 
	padding: 5px 0;
}

div.partner-info.input input,
div.partner-info.input p
{
	width: 378px !important;
	height: 30px; 
	font-size: 14px; 
	padding: 5px 0; 
	padding-left: 5px;
	border: 1px solid #E3E3E3;
	background-color: #FFFFFF;
}

p#partner-name
{
	height: auto;
	margin-top: 5px;
	padding: 5px 10px 5px;
	color: #999;
}

p#partner-description
{
	width: 593px !important;
	margin-top: 5px;
	margin-left: 10px;
	padding-left: 10px;
  	padding-top: 10px;
	height: 60px;
  	overflow: scroll;
  	overflow-x: hidden;
}

div.partner-info.description.label
{
	margin: 10px 0 0 10px;
	clear: both;
	text-align: left; 
	font-weight: bold !important;
}

div.partner-info.description.label p
{  
}

div.partner-info.input textarea
{
	width: 545px !important;
	height: 60px;
	margin: 10px;
	padding: 10px;
	font-size: 14px; 
	border: 1px solid #E3E3E3;
	background-color: #FFFFFF;
}


div.partner-info .ui-selectonemenu {
	background:none;
}
 
div.partner-info .ui-selectonemenu-trigger {
	background: url(../resources/img/arrow_down.png) no-repeat 96% 12px !important;
}

#partnerId_btn {
	
	padding: 5px 19px 4px !important;
	display:block;
	float:left;	
	margin: 10px 0 10px 4px;
}

.partner-info.input select{
	padding: 3px 0 !important;


	height: auto;
	color: #999;
}