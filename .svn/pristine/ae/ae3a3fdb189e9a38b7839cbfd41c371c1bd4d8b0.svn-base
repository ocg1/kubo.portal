package mx.com.kubo.bean;

import java.io.Serializable;

public class PreguntaFrecuente implements Serializable{

	private String pregunta;
	private String direccion;
	private String dirPreg;
	
	
	public String getDirPreg() {
		return dirPreg;
	}
	public void setDirPreg(String dirPreg) {
		this.dirPreg = dirPreg;
	}
	public String getPregunta() {
		return pregunta;
	}
	public void setPregunta(String nombre) {
		this.pregunta = nombre;
	}
	public String getDireccion() {
		return direccion;
	}
	public void setDireccion(String direccion) {
		this.direccion = direccion;
	}

}
