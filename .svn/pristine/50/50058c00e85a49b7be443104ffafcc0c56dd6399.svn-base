	vertical-align: baseline;
	font-size:15px;

}
.alerts > p > a{
	font-size:14px;
	color:#548f39;
	text-decoration:underline;
}
.show {
	-webkit-transform: scale(1);
	-moz-transform: scale(1);
	-o-transform: scale(1);
	transform: scale(1);
	opacity: 1;
}
