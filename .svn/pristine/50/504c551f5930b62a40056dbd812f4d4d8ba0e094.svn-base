<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
	<li>
		<label class="etiqueta">Cuenta: </label>
		
			<div class="campo">
				<div class="styled-select select_border">
					<h:selectOneMenu id = "cuentasUsrRetiro" 
								     tabindex   = "1" 
								     styleClass = "elementStlSelAddress" 
								     value      = "#{disposicionEfectivo.cuentaActual}">  
								     
				        <f:selectItem itemValue = "" itemLabel = "Todas las cuentas" />
				        
				        <f:selectItems value = "#{disposicionEfectivo.listInvAccounts}" 
				                       var   = "account" 
				                       itemLabel = "#{account.accountName}" 
				                       itemValue = "#{account.account}"/>
				                       
				        <p:ajax event = "change" listener = "#{disposicionEfectivo.changeAccount}" immediate = "true"/>    
				    </h:selectOneMenu>
				</div>
			</div>	
		
	</li>				
</ui:composition>				