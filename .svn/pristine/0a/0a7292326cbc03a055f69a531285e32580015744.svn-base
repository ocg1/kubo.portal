<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui">

	<h:form>
	<div id="mycontent" class="fondoBody">
		<div class="fondoDer">
			<div class="fondoIzq">
				<div id="mycontent" class="General">
					<h:panelGrid columns="1" width="100%" columnClasses="columnas"
						cellspacing="15px">
						<h:panelGrid columns="2">
							<h:panelGroup>
								Busqueda
							</h:panelGroup>
							<h:panelGroup>
								Busqueda por Categoria
							</h:panelGroup>
							<h:panelGroup>
								<h:inputText id="Busqueda"></h:inputText>&nbsp;
								<p:commandButton value="Ir"></p:commandButton>&nbsp;&nbsp;&nbsp;&nbsp;
							</h:panelGroup>
							<h:panelGroup>
								<h:form>
									<h:panelGroup>
										<h:selectOneMenu id="categoria"
											value="menuHelpController.actual" onchange="submit()"
											valueChangeListener="#{menuHelpController.valueMenuChanged}"
											style="selectonemenu">
											<f:selectItems value="#{menuHelpController.menus}" />
										</h:selectOneMenu>
									</h:panelGroup>
								</h:form>
							</h:panelGroup>
						</h:panelGrid>
						<h:panelGroup>
							<hr class="hrCnt"></hr>
						</h:panelGroup>
						<h:panelGroup>
							<div style=" margin-left: auto; margin-right: auto;  width: auto;">
								<div  id="preguntas" style=" margin-left: auto; margin-right: auto;">
									<h:form id="form">  
  
									    <p:tree id="docTree" value="#{documentsController.root}" style="width: 900px; margin-left: auto; margin-right: auto;" var="doc">  
									        <p:treeNode expandedIcon="ui-icon-folder-open"  
									                    collapsedIcon="ui-icon-folder-collapsed">  
									            <h:outputText value="#{doc.name}"/>  
									        </p:treeNode>  
									  
									        <p:treeNode type="document" icon="ui-icon-document">  
									            <h:outputText value="#{doc.name}" />  
									        </p:treeNode>  
									  
									        <p:treeNode type="picture" icon="ui-icon-image">  
									            <h:outputText value="#{doc.name}" />  
									        </p:treeNode>  
									  
									        <p:treeNode type="mp3" icon="ui-icon-video">  
									            <h:outputText value="#{doc.name}" />  
									        </p:treeNode>  
									    </p:tree>  
									  
									</h:form>  
								</div>
							</div>
						</h:panelGroup>
						<h:panelGroup>
							<hr class="hrCnt"></hr>
						</h:panelGroup>
					</h:panelGrid>
				</div>
			</div>
		</div>
	</div>
	</h:form>
</html>





