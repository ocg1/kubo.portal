<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:c  = "http://java.sun.com/jsp/jstl/core"
				xmlns:p  = "http://primefaces.org/ui">
				
	<h:panelGroup rendered = "#{summaryRequest.prospecto.area == 'L'}" >
		<div class = "clear details4 left">
			<p style = "float:left"><strong>Actividad econ√≥mica</strong></p>
			
			<ui:include src = "tooltip/actividad_economica.xhtml"/>
					
			<ui:include src = "actividad_economica/domicilio/alarma-coincidencia.xhtml"/>	
			<ui:include src = "actividad_economica/domicilio/alarma-coincidencia-nivel-2.xhtml"/>		
			
			<div style = "clear: both;"/>
			
			<ui:repeat var = "listEmployment" value = "#{summaryRequest.listEmployment}">
				<p>#{summaryRequest.female?'Empleada':'Empleado'} de #{listEmployment.employer} por #{summaryRequest.actividad_economica.auxTenure} 
					<h:panelGroup rendered="#{summaryRequest.nameVisible}">
					<br />Ubicado en #{summaryRequest.actividad_economica.employAddressStr}
					</h:panelGroup> 
				</p>
			</ui:repeat>
			
			<ui:repeat var = "listBusiness" value = "#{summaryRequest.listBusiness}">
				<p>#{summaryRequest.female?'Empresaria':'Empresario'} de "#{listBusiness.business_name}" por #{summaryRequest.actividad_economica.auxYears}
				
					<h:panelGroup rendered = "#{summaryRequest.nameVisible}">
					 	<br />Ubicado en #{summaryRequest.actividad_economica.businessAddressStr} 
					 </h:panelGroup>
				 </p>
			</ui:repeat>    			
			
			<ui:include src = "actividad_economica/domicilio/lista_coincidencias.xhtml"/>				
	
		</div>
	</h:panelGroup>				
</ui:composition>				