<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:p  = "http://primefaces.org/ui">
				
				
				
	<div style = "display: none">
		<h:inputText id = "init-domicilio" immediate = "true" >
			<p:ajax event = "click" update = "coverage-zone-ENABLED" oncomplete = "Domicilio.domicilio_on_complete_nuevo();" />
		</h:inputText>		
		
		<h:inputText id = "init-neighborhood-text-ENABLED" immediate = "true" ajax = "true">
			<p:ajax event = "click" update = "coverage-zone-ENABLED"
					oncomplete = "Domicilio.neighborhood_text_ENABLED_on_complete(xhr, status, args);"
					listener = "#{basicData.init_neighborhood_text_ENABLED}"/>
		</h:inputText>	
		
		<h:commandLink id = "focus-domicilio">
			<p:ajax event = "click" 
					oncomplete = "Domicilio.focus_on_complete(xhr, status, args);"
					listener = "#{basicData.domicilio.init_focus_date}"/>
		</h:commandLink>
	</div>				
</ui:composition>				