					$("#valProspectus").val(prospectus_id).blur();
					$("#valCoach").val(coach).blur();
					$("#valCompany").val(company).blur();
					$("#valFrom").val(from).blur();
				
					$("#cmdChargeInf").click();
				
				}else{
					
					console.log( "No ejecuto...... inicio" );
					
				}
				
		}
	
	
