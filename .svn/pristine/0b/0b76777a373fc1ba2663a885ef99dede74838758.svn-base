<h:body>
	
		<h:form prependId="false">
			
			<div id="header" class="head_coach">
			
				<div class="inner_block">
					  	<div class="datos_prospecto">
						  	
						  	<p><strong>PRUEBA TOKEN</strong> </p>
						    
						</div>
							
				</div>
			</div>
			
		</h:form>


	<h:form prependId="false">
	
		<div style="width: 450px; height: 500px; margin-left: auto; margin-right: auto; border: solid 1px #E6E6E6; margin-top: 100px;"  >
		
			<h:panelGroup id="pnlToken">
			
				<h:panelGroup rendered="#{pruebaToken.status == 0}" >
				
					 <h:commandLink 
					 	styleClass="btn_continuar" 
					 	value="Genera Token" 
					 	
					 	>
					 	
					 	<p:ajax event="click" listener="#{pruebaToken.generaToken}" update="pnlToken" immediate="true"  ></p:ajax>
					 	
					 </h:commandLink>
				
				</h:panelGroup>
				
				<h:panelGroup rendered="#{pruebaToken.status == 1}" >
				
					<h:panelGroup rendered="#{pruebaToken.envioToken}" >
				
						<div style="display: block;width: 448px; margin-top: 15px;">
							<div style="float: left;width: 34px;" >
							
								<p:graphicImage value="../resources/img/Send-Mail-32.png" ></p:graphicImage>
							
							</div>
							<div style="float: left; font-size: 1.15em; padding-top: 5px; padding-left: 4px;" >
								Se ha enviado un Código de seguridad al telefono ****3209
							</div>
						</div>
						
						<div style="display: block;width: 448px; margin-top: 15px; clear: both; float: none;font-size: 1.15em;">
						
							<div>
								Ingresa tu código:
							</div>
							
							<div>
								<h:inputText style="font-size: 1.15em;" value="#{pruebaToken.token}" >
									<p:ajax event="blur" ></p:ajax>
								</h:inputText>
							</div>
							
							<h:commandLink 
							 	styleClass="callAction btnNaranja" 
							 	value="VALIDAR" 
							
							 	>
							 	
							 	<p:ajax event="click" listener="#{pruebaToken.validaToken}" update="pnlToken" immediate="true"  ></p:ajax>
							 	
							 </h:commandLink>
						
						</div>
					
					</h:panelGroup>
					
					<h:panelGroup rendered="#{!pruebaToken.envioToken}" >
					
						<div style="display: block;width: 448px; margin-top: 15px; ;clear: both; float: none;">
							<div style="float: left;width: 34px;" >
							
								<p:graphicImage value="../resources/img/warning.png" ></p:graphicImage>
							
							</div>
							<div style="float: left; font-size: 1.15em; padding-top: 5px; padding-left: 4px;" >
								Ocurrió un error al generar el token ****3209
							</div>
						</div>
					
					</h:panelGroup>
				
				
				</h:panelGroup>
				
				
				<h:panelGroup rendered="#{pruebaToken.status == 2}" >
			
					<h:panelGroup rendered="#{pruebaToken.validToken}"  >
					
						<div style="display: block;width: 448px; margin-top: 15px;clear: both; float: none;">
							<div style="float: left;width: 34px;" >
							
								<p:graphicImage value="../resources/img/check.png" width="32px" height="32px" ></p:graphicImage>
							
							</div>
							<div style="float: left; font-size: 1.15em; padding-top: 5px; padding-left: 4px;" >
								El código es correcto: #{pruebaToken.msg}
							</div>
						</div>
					
					</h:panelGroup>
					
					<h:panelGroup rendered="#{!pruebaToken.validToken}" >
					
						<div style="display: block;width: 448px;  margin-top: 15px;clear: both; float: none;">
							<div style="float: left;width: 34px;" >
							
								<p:graphicImage value="../resources/img/error.png" width="32px" height="32px" ></p:graphicImage>
							
							</div>
							<div style="float: left; font-size: 1.15em; padding-top: 5px; padding-left: 4px;" >
								El código es incorrecto: #{pruebaToken.msg}
							</div>
						</div>
					
					</h:panelGroup>
				
				</h:panelGroup>
			
			</h:panelGroup>
		
		</div>
	
	</h:form>
	

	<h:panelGroup id="pnlCancel" >
	</h:panelGroup>

</h:body>
