package mx.com.kubo.notificaciones.notificacion;

public enum NotificacionTipoEnvio 
{
	TO("TO"),
	CC("CC"),
	CCO("CCO");
	
	private String descripcion;
	
	private NotificacionTipoEnvio(String descripcion)
	{
		this.descripcion = descripcion;
	}
	
	@Override
	public String toString()
	{
		return descripcion;
	}
}
