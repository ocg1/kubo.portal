<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
	<style>
		#container_buscar 
		{
			float: left; 
			margin-left: 40px; 
			margin-top: 44px;
			border: 0px solid silver;
		}	
	</style>				
	
	<div id = "container_buscar" onclick = "Busqueda.init_summary_request()">																												
			<div class = "buttonPR" style = "margin:5px 0;width: 100px;">	
				<h:outputText value="Buscar" />					
			</div>
	</div>	
	
	<div style = "display: none">
		<h:commandLink id = "init-summary-request" immediate = "true">
			<p:ajax event = "click" update = ":update_summary :pnlCancel"
					onstart    = "displayMessageProcessing('msgprocessing',false);"
					oncomplete = "Busqueda.completSearch(xhr, status, args);"
					listener   = "#{searchRequest.coach_engine.init_summary_request}"/>
		</h:commandLink>
	</div>	
	
	<div style = "clear: both;" />				
</ui:composition>		