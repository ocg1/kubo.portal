<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
			     xmlns:p  = "http://primefaces.org/ui"
			     xmlns:f  = "http://java.sun.com/jsf/core">
			     
	<div class = "dvContent">
	
		<div class = "dvLabel" >
			<h:outputLabel styleClass = "labelsStl" value = "#{msg.residence_type} " />
		</div>
		
		<P />
		
		<div  class = "dvElementAddres1">
			<div class = "selectNuevo"  style="border: solid 0px !important; height: 13px !important;" >
				<h:selectOneMenu immediate  = "true"
								 value      = "#{benefic.editor_vivienda.tipo_vivienda_id}" 
								 styleClass = "elementStlSelAddress validatorClass mapOff">
								 
					<f:selectItem itemLabel = "" itemValue = "" />
					
					<f:selectItems  value     = "#{benefic.editor_vivienda.lista_tipo_vivienda}" 
									var       = "tipo_vivienda" 
									itemLabel = "#{tipo_vivienda.description}" 
									itemValue = "#{tipo_vivienda.residencePK.residence_id}"/>
									
					 <p:ajax event = "change" listener = "#{benefic.editor_vivienda.listener_tipo_vivienda_SELECTED}"/>
					 
				</h:selectOneMenu>
			</div>
		</div>
	</div>			     
</ui:composition>