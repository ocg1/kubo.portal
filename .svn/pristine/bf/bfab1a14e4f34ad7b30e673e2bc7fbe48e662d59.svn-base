@CHARSET "UTF-8";

.tituloCnt 
{	
	width: 100%;
	height: 100px; 
	
	margin-bottom: 32px;
	padding:0; 
	
    float: left;
    font-size: 2.38em;
    
	font-weight: bold;
	line-height: 35px;
	
	overflow: hidden; 

	border:0px solid black;
}


#contCap1Img{
	width: auto;
	margin-left: auto;
	margin-right: auto;
	height: auto;
	overflow: auto;
}
#contCap1Img1,#contCap1Img2,#contCap1Img3{
	float: left;
	width: 30%;
	margin-top: 10px;
	margin-left: 8px;
	margin-right: 8px;
	height: auto;
	overflow: auto;
}

.tituloMenu{
	padding-top: 5px;
	height: 29px;
	vertical-align: bottom;
	text-align: inherit;
}
.txtImg, .lsCnt  {
	/*color: #0A2945;*/
	font-weight: normal;
	text-decoration : none;
	vertical-align: top;
	font-size: 1.1em;
}
.FraseAyuda{
	font-size: 1.05em;
	font-weight: bold;
	color: #2E2F31;
	vertical-align: middle;
	text-align: center;
}

.lsCnt
{
	text-align: justify;
	font-size: 1.15em;
	
	line-height: 20px;
	letter-spacing:0;
}

.CntDesc{
	margin-left: 10px;
	padding-left: 10px;
}
.lsCntDesc, .CntDesc{
	font-size: 1.1em;
}
.lsCnt H3{
	font-size: 138.5%;
	margin-bottom: 5px;
	line-height: 28px;
}
.lsCnt H2{
	font-size: 146.5%;
	font-weight: bold;
	line-height: 28px;
}
ul{
	list-style-image: url(../../../../resources/img/Bullet.png);
}
.calltoAction{
	font-size: 1.08em;
	font-weight: normal;
	color: #353D44;
	vertical-align: middle;
	text-align: center;
}
.cierre{
	font-size: 0.87em;
	font-weight: normal;
	color: #353D44;
	vertical-align: middle;
	text-align: center;
}
.linkInterno {
	color: #2D8F13;
	text-decoration : none;
}
.link2{
	
	color: inherit;
	text-decoration: none;
	font-size: inherit;
}
