<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
	 <h:panelGroup id = "pnlCanceled" rendered = "#{summaryRequest.member.is_canceled != null and (summaryRequest.member.is_canceled == 'SC' or summaryRequest.member.is_canceled == 'SK' )}" >
		
		<p:commandLink style="text-decoration:none;" 
					   onstart="return initActiveAccount();" 
					   immediate="true" 
					   actionListener="#{summaryRequest.activeAccountAction}" 
					   oncomplete="completeCancelAction();"
					   value="Activar Cuenta" >
		
		
		
		</p:commandLink>
	
	</h:panelGroup>				
</ui:composition>				