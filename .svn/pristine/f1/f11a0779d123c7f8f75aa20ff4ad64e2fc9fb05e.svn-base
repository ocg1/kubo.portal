<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
			     xmlns:f  = "http://java.sun.com/jsf/core"
				 xmlns:p  = "http://primefaces.org/ui">	

	<div class = "field">
		<div class = "dvLabel">
			<h:outputLabel styleClass = "labelsStl" value = "#{msg.birthday} "/>
		</div>
	
	
		<div>
			<table class="tabla_fechanacimineto">
				<tr class="myBirthday">									
					<td>
						<div class="selectNuevo">
						<h:selectOneMenu   value="#{benefic.day}"  styleClass="elementStlSelDay validatorClass requiredVerify">
							<f:selectItem itemLabel="Día: " itemValue="0" />
							<f:selectItems  value="#{ibeneficiaries.beneficiario.days}"/>
							<p:ajax event="change" listener="#{ibeneficiaries.beneficiario.init_date_of_birth(benefic)}"/>
						</h:selectOneMenu>
						</div>
					</td>
				
					<td>
						<div class = "selectNuevo">
							<h:selectOneMenu  value="#{benefic.month}" styleClass="elementStlSelMonth validatorClass requiredVerify">
								<f:selectItem itemLabel="Mes: " itemValue="0" />
								<f:selectItems  value="#{ibeneficiaries.beneficiario.months}"/>
								<p:ajax event="change" listener="#{ibeneficiaries.beneficiario.init_date_of_birth(benefic)}"/>
							</h:selectOneMenu>
						</div>
					</td>
					
					<td>
						<div class="selectNuevo">
							<h:selectOneMenu  value="#{benefic.year}"  styleClass="elementStlSelYear validatorClass requiredVerify">
								<f:selectItem itemLabel="Año: " itemValue="0" />
								<f:selectItems  value="#{ibeneficiaries.beneficiario.years}"/>
								<p:ajax event="change" listener="#{ibeneficiaries.beneficiario.init_date_of_birth(benefic)}"  />
							</h:selectOneMenu>
						</div>
					</td>
				</tr>
			</table>															
		</div>
	</div>	
</ui:composition>			