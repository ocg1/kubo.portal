<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
			     xmlns:p  = "http://primefaces.org/ui">
	<P style="clear: both;" />
	
	<div class="dvContent" style="width: 380px;">
		<div class="dvLabel" style="width: 120px; clear: both;">
			<h:outputLabel styleClass="labelsStl" value="#{msg.BetweenStreet1_text}" />
		</div>
		
		<P />
		
		<div class="dvElementAddres1">
			<h:inputText id    = "first_street_reference_#{benefic.beneficiarie_id}" 
						 value = "#{benefic.editor_vivienda.address.first_street_reference}"  
			             styleClass = "elementAddres1 validatorClass">
				<p:ajax event      = "blur" 
					    listener   = "#{benefic.editor_vivienda.listener_generar_vivienda_TOKEN}"
					    oncomplete = "Vivienda.callback_vivienda_TOKEN(xhr, status, args);"/>
			</h:inputText>
		</div>
	</div>			     
</ui:composition>