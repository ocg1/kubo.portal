<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
				 xmlns:f  = "http://java.sun.com/jsf/core"
				 xmlns:p  = "http://primefaces.org/ui">	

	<div style = "
				  display: #{m.isConBlocked}; ">
	
		<p:commandLink	immediate      = "true" ajax = "true"		
						onclick        = "changePage();"
						
						onstart        = "return revision_pasos('#{m.idItem}');"
						actionListener = "#{navigationBean.cambiaPagina}"	
						oncomplete     = "closeMessageProcessing();"																					
						update         = ":form_Prin :actualPage :form_Prin:hiddenactios :contSimReg">
						
				<div id = "#{m.idItem}" class = "itemMenu #{m.classItem}">
					<div class = "item_number">		
						<i class="fa fa-check"></i>			
						<div class = "#{m.numItem} item_position">#{m.position}</div>
					</div>		
					
					<div class="menu_b" >
					
					
						<div class = "#{m.classItem}Cont">#{m.nameItem}</div>
						
				
					<!-- h:panelGroup rendered = "#{m.displayBar and sessionBean.area == 'I'}" -->
						
							<div id = "#{m.idItem}ItemBar" class = "displayNone">
							
								<p:progressBar value = "#{m.porcItem}"></p:progressBar>
								
										<div id = "#{m.idItem}Porcent" class = "#{m.porcClassItem}">
											#{m.porcItem}% <span>de avance</span>
										</div>
									
							</div>
					<!-- /h:panelGroup -->
					</div>
					
					<f:attribute name = "section" value = "#{m.targetItem}::#{m.screenid}::#{m.idItem}" />
					 
				</div>
		</p:commandLink>
		
		
	</div>		
	 				 
</ui:composition>				 