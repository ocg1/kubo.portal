<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"				 
				 xmlns:p  = "http://primefaces.org/ui">	
				 
	<div id = "system-log" class = "notificacionListado">
		<ol>
			<li class="encFechaNotificacion">Fecha</li>
			<li class="encDescripcionNotificacion">Descripción</li>
		</ol>
		
		<ui:repeat value = "#{notificaciones_sistema.bitacora}" var = "notificacion">
			<ul>
				<li class="FechaNotificacion" ><strong><sub class="caption">Fecha</sub>#{notificacion.notification_date}</strong></li>
				<li class="DescripcionNotificacion" ><sub class="caption">Descripción</sub>#{notificacion.comments}</li>
			</ul>
		</ui:repeat>
	</div>				 
</ui:composition>				 