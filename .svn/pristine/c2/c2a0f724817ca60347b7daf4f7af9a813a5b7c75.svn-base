			function mensajeInvCuenta() {
			var idClienteInvestor = $(".select_cuenta select").val();
							
			var cuentas = ['00100011760','00100013177','00100015226','00100016176','00100016389','00100016575','00100019221'];
			    if($.inArray(idClienteInvestor, cuentas) >= 0) {
			    	$(".mensajeInversionista").slideDown();
			    	
			    }else {
			    	$(".mensajeInversionista").hide();
			    }
			
			
			
			var cuentas2 = ['00100023997','00100037157','00100014963','00100015510','00100008777'];
		    if($.inArray(idClienteInvestor, cuentas2) >= 0) {
		    	$("#raContent").show();
		    	$("#raContent").css({
		    		"height": "auto",
		    		"opacity": "1"
		    	});
		    	
		    	
		    }else {
		    	$("#raContent").remove();
		    }
		} 
		</script>
		
	
		
		
