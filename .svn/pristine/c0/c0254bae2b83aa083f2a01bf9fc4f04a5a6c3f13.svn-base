	display:block;
	position:absolute;
	z-index:9999999999999999999999999;
	
	border: solid 2px #548f39;
	top:30px;
	width: 680px;
	-webkit-transition: .4s;
	-moz-transition: .4s;
	-o-transition: .4s;
	transition: .4s;
	-webkit-transform:scale(0);-moz-transform:scale(0);-o-transform:scale(0);
	transform: scale(0); 
	opacity: 0;	
	padding:20px 20px 20px 20px;
	
}



