<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
				
		<div style = "display: none;">
		
			<h:form prependId = "false">
			
				<p:commandLink id	= "retiroAction" 
							   styleClass		= "call_action"
							   value 			= "Retirar" 
							   actionListener	= "#{disposicionEfectivo.notifica()}" 
							   onstart			= "return displayMessageProcessing('msgprocessing',false);" 
							   oncomplete		= "resRet(xhr, status, args);"
							   immediate		= "true"/>
			
				<p:inputText id = "ammountRet" value = "#{disposicionEfectivo.monto_SELECTED}">
				     <p:ajax event = "blur" />
				</p:inputText>
	
<!-- 				
				<p:commandLink id = "notificar-cancelacion-cuenta"
							   immediate = "true"
							   actionListener = "#{cancelacionCuenta.notificar}">						   				
				</p:commandLink>		
-->				
			</h:form>		
		</div>						
</ui:composition>				