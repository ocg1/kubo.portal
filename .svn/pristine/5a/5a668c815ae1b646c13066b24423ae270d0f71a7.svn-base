package mx.com.kubo.model;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Embeddable;

@Embeddable
public class ReferencesMotivePK implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	@Column
	private int company_id;
	@Column
	private int reference_motive_id;
	
	public int getCompany_id() {
		return company_id;
	}
	public void setCompany_id(int company_id) {
		this.company_id = company_id;
	}
	public int getReference_motive_id() {
		return reference_motive_id;
	}
	public void setReference_motive_id(int reference_motive_id) {
		this.reference_motive_id = reference_motive_id;
	}
}
