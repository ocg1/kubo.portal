<ui:composition xmlns   = "http://www.w3.org/1999/xhtml"
			    xmlns:h = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
	<div class="field">
		<div class="dvLabel">
			<h:outputLabel styleClass="labelsStl monto_maximo" value="¿Cuál será el monto máximo de cada uno de tus retiros?*"/>
		</div>
		<div class="leyendas italic">Si necesitas retirar más, lo podrás hacer.</div>
	
			<h:inputText id="momentmaxret" maxlength="10" onkeyup="format(this);"  style="text-align: right;"    value="#{addPldDocument.maximum_withdrawStr}" styleClass="elementAddres1 formatpld validatorClass hs_class" >
			<p:ajax event="blur" listener="#{addPldDocument.savePrevencionLD('maximum_withdrawStr')}"></p:ajax>
			<f:convertNumber pattern="###,###,###,###" locale="es_MX" type="currency" groupingUsed="true" />
			</h:inputText>
		
	</div>				
</ui:composition>				