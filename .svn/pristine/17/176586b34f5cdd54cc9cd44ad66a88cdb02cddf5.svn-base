<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
	<div class="dvContent2">

			<h:outputText value="#{msg.pr_secondName}" styleClass="etiqueta" />
		
		

			<p:inputText id = "second_name" 
						 onfocus = "$(this).removeClass('requiredClass');" 
						 value   = "#{addNaturalPerson.newNaturalPerson.middle_name}" 
						 styleClass = "elementStl" >
				<p:ajax event = "blur"/>
			</p:inputText>
			<p:watermark for="second_name" value="Otros"  />
			<!-- 
			<p:tooltip for = "second_name" 
					   styleClass = "tooltipStl" 
					   value = "Escribe aquí tu(s)  nombre(s) siguente(s), en caso de no contar con algún otro, deje este elemento vacio " 
					   showEvent  = "focus" 
					   hideEvent  = "blur"  
					   showEffect = "slide" 
					   hideEffect = "slide" />
	 -->
	</div>				
</ui:composition>				