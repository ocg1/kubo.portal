<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
				 xmlns:p  = "http://primefaces.org/ui">					 
	
	<div class = "dvContent">
		<div class = "dvLabel" style = "margin: 0;">
			<h:outputLabel styleClass = "editor-domicilio label" value = "#{msg.Zipcode_text}" />
		</div>					
					
		<div id = "employment-address-zipcode">					
			<h:inputText id = "zip_code" maxlength = "5" immediate = "true"  	
						 value = "#{summaryRequest.domicilio_actividad.address.zip_code}"					 						 						 
						 onkeypress = "return isNumberKey(event);"   
						 styleClass = "elementAddres1 validatorClass mapClass zipcode editor-domicilio input-text">
						 
				<p:ajax event = "change" 
						onstart    = "return validateZipCode('employment-address-zipcode');" 
						oncomplete = "ActividadDomicilio.zipcode_oncomplete(xhr, status, args);"  
						listener   = "#{summaryRequest.domicilio_actividad.init_zipcode}" />
			</h:inputText>									
		</div>
	</div>	
</ui:composition>