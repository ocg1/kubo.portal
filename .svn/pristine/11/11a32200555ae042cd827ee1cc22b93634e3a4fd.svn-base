<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
				
	<div>
		<div class = "styled-select">															

			<h:selectOneMenu id = "select-type_phone" 
							 value = "#{summaryRequest.telefono.newAddPhone.phone_type_id}"
							  
						     styleClass = "elementStlSelAddress" >
						     
				<f:selectItem itemValue = ""  itemLabel = ""/>
				
				<f:selectItems value = "#{summaryRequest.listPhoneType}" 
							   var   = "phonetype" 
							   itemLabel = "#{phonetype.name}" 
							   itemValue = "#{phonetype.phoneTypePk.phone_type_id}"/>
							   
				<p:ajax event = "change" onstart = "addnumladacel('select-type_phone');"/>															
			</h:selectOneMenu>																						
		</div>
	</div>	
</ui:composition>				