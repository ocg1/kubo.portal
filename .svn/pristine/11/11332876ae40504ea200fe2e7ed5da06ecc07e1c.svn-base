<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
				 xmlns:f  = "http://java.sun.com/jsf/core"
				 xmlns:p  = "http://primefaces.org/ui">	
				 
	<h:panelGroup rendered = "#{summaryRequest.editor_identification.label_ENABLED}">
		<p>#{summaryRequest.editor_identification.label}</p>
	
		<h:panelGroup rendered = "#{summaryRequest.editor_identification.identification_type_id == 2}">	
			<h:inputText id = "mx-ine-cic" immediate = "true" value = "#{summaryRequest.persona.mx_ine_cic}"> 
													   
					<p:ajax event = "change"
					  		oncomplete = "IdentificationType.edit_oncomplete(xhr, status, args);"
							listener = "#{summaryRequest.editor_identification.edit}"/>
			</h:inputText>
		</h:panelGroup>
		
		<h:panelGroup rendered = "#{summaryRequest.editor_identification.identification_type_id > 2}">
			<h:inputText id = "mx-ife-cveelector" immediate = "true" value = "#{summaryRequest.persona.mx_ife_cveelector}"> 
													   
					<p:ajax event = "change"
					  		oncomplete = "IdentificationType.edit_oncomplete(xhr, status, args);"
							listener = "#{summaryRequest.editor_identification.edit}"/>
			</h:inputText>
		</h:panelGroup>		
	</h:panelGroup>			 
</ui:composition>		