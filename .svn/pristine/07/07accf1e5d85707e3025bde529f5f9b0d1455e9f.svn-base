<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
	  			xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
		
	<div class="tablaVerde">
	<i class="fa fa-caret-down "></i>
	<table cellspacing="0" cellpadding="0" border="0">
	
	<tr>
			<td style="text-align:right;">
				
				<h:outputText value="Número de pagos" 	styleClass="labelsStl clLabelTable" />
	            
			</td>
			
			<td style="text-align:left;">
			
				<h:outputText value="#{deal.selectedDeal.totalCuotas} pagos" 	styleClass="labelsStl  clLabelTableVal" />
			
			</td>
		</tr>
		 
		<tr>
			<td style="text-align:right; vertical-align: top; ">
			
				<h:outputText value="Pagos #{deal.selectedDeal.feeStrDesc} de: " 	styleClass="labelsStl clLabelTable"  />
				
			</td>
	                	
			<td style="text-align:left;">
				<h:outputText value="#{deal.selectedDeal.feeStrVal} "	styleClass="labelsStl  clLabelTableVal" />
			</td>
		</tr> 
	
	</table>
</div>
	<table class="propuesta_tblPropuesta">	           
		
	    <tr>
			<td style="text-align: right; padding-top: 15px; ">
	        	<h:outputText value="Tasa: " styleClass="labelsStl clLabelTable" />
			</td>
	                	
			<td style="text-align:left; padding-top: 15px;">
				<span class="labelsStl  clLabelTableVal">#{deal.selectedDeal.rate}<sup class="super">(a)</sup></span>
			</td>
		</tr>    
			<tr>
			<td style="text-align:right; vertical-align: top; ">
			
				<h:outputText value="Comisión por apertura: " 	styleClass="labelsStl clLabelTable"  />
				
			</td>
			
			<td style="text-align:left;">
				<h:outputText value = "#{deal.selectedDeal.montoComision} %"	styleClass="labelsStl clLabelTableVal" />
			</td>
		</tr>   
		<tr>
			<td style="text-align:right; ">
	                	
				<h:outputText value="Monto a recibir: " 	styleClass="labelsStl clLabelTable" />
				
			</td>
			
			<td style="text-align:left;">
	        	<h:outputText value = "#{deal.selectedDeal.amountReal}" styleClass="labelsStl  clLabelTableVal" />
			</td>
		</tr>	
		
		
		
		<tr>
			<td style="text-align:right;">
				    
				<h:outputText value="Total a pagar: " 	styleClass="labelsStl clLabelTable" />
			</td>
			
			<td style="text-align:left;">
				<h:outputText value="$ #{deal.selectedDeal.totalStr}"  	styleClass="labelsStl  clLabelTableVal" />
			</td>
		</tr>
		
		<tr>
		<td style="text-align:right;">
			<h:outputText value="CAT" 	styleClass="labelsStl clLabelTable"  />
		</td>
		<td style="text-align:left;">
	
			 <div class="clLabelTableVal">#{deal.selectedDeal.catStr} sin IVA<sup style="font-size: 0.9em !important;">(b)</sup></div>
	
	    </td>
		
		</tr>
		              
	
	</table> 

</ui:composition>			                	 