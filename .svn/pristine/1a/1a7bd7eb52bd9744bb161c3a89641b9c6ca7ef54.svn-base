<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
				
	<div class = "field">
		<div class = "dvLabel">
			<h:outputLabel styleClass = "labelsStl" value = "#{msg.Zipcode_text}" />
		</div>
		
	
		
		<div id = "input_zip_code" class="dvElementAddres1">					
			<h:inputText id = "zip_code" maxlength = "5" tabindex = "27"  						 
						 value = "#{employment.addressbean.zip_code}" immediate = "true"
						 onfocus = "ActividadEconomica.Domicilio.init_focus_date(4);"
						 onkeypress = "return isNumberKey(event);"   
						 styleClass = "elementAddres1 validatorClass mapClass zipcode hs_class">
						 
				<p:ajax event = "change" 
						onstart    = "return validateZipCode('uiemp\:#{status.index}\:zip_code');" 
						oncomplete = "ActividadEconomica.Domicilio.zipcode_oncomplete(xhr, status, args);"  
						listener   = "#{addMyLoan.domicilio.init_zipcode}" />
			</h:inputText>
						
			<h:inputText value = "#{employment.addressbean.latLong}" 
						 style = "display: none;" 
						 styleClass = "latlongClass">
						 
				<p:ajax event = "blur" listener = "#{addMyLoan.domicilio.setSplitLatLongEmployment(employment)}"/>
			</h:inputText>						
		</div>
	</div>				
</ui:composition>