				xmlns:ui = "http://java.sun.com/jsf/facelets"				
				xmlns:p  = "http://primefaces.org/ui">
				
	<div class = "dvContent stlCntNumApple">
			<div class="dvLabel" >
				<h:outputLabel styleClass="labelsStl" value="${msg.Block_text}" />
			</div>

			
			<div class = "dvElement">
				<h:inputText id = "mx_manzana" immediate = "true" tabindex = "32" 
				             value = "#{employment.addressbean.mx_manzana}" 
				             onfocus = "ActividadEconomica.Domicilio.init_focus_date(4);"
				             styleClass = "elementAddresMin">
				             
					<p:ajax event = "blur"
							oncomplete = "ActividadEconomica.Domicilio.input_text_oncomplete(xhr, status, args);" 
							listener   = "#{addMyLoan.domicilio.init_input_text}"/>
				</h:inputText>
			</div>
		</div>				
</ui:composition>				