	</h:head>
	<h:body>
		
			<div id="header2" align="center" style="width: 100%; magin-right: 0px; margin-bottom: 0px;">
				<p:graphicImage 
							url="../../resources/img/logo002.png"></p:graphicImage>
			</div>
			<div id="header" style="width: 100%; magin-right: 0px; margin-bottom: 0px;">
				<ui:include src="templates/headContenido.xhtml" />
			</div>
			
		<div id="general">
			<div id="opciones">
				<h:form>
					<div class="opc">
						<p:commandLink action="proyectos" styleClass="linksQuestMenu">
							<span class="resalte">Descubre</span><br /> grandes proyectos
						</p:commandLink>
					</div>
					<div class="opc">
						<p:commandLink action="preregistro" styleClass="linksQuestMenu">
							<span class="resalte">Registrate</span><br /> con nosotros
						</p:commandLink>
					</div>
					<div class="opc">
						<p:commandLink action="regProyecto" styleClass="linksQuestMenu">
							<span class="resalte">Comienza</span><br /> tu proyecto
						</p:commandLink>
					</div>
					<div class="opc">
						<span class="resalte">Ayuda</span>
					</div>
				</h:form>
			</div>	
			<div id="contentCurated">
				<div id="imgCurated">
					<p:graphicImage url="../resources/img/#{curatedController.imgURL}"></p:graphicImage>
				</div>
				<div id="curatedDesc">
					<h:form>
					<div id="curatedName">#{curatedController.cuartedName}</div>
					<div id="curatedDescrip">#{curatedController.cuartedDesc}</div>
					<div id="curatedLink"><p:commandLink>#{curatedController.cuartedURL}</p:commandLink></div>
					</h:form>
				</div>
			</div>
		</div>
		<div class="separador">
			<hr />
		</div>
		<div id="proyCont" align="center">
		<h:form>
		<ui:repeat id="searchResults" var="pr" value="#{curatedController.proyectos}" >
			<h:panelGroup style="float: left; display: inline-table; width: 206px;">
				<div id="proyectos">
					<div id="proyecto" >
						<div id="imagen">
							<p:graphicImage width="200px" height="200px" url="../resources/img/#{pr.urlImg}"></p:graphicImage>
						</div>
						<div id="calif">
							<div id="yui-gen134" class="yui-dt-liner">
								<span id="yui-gen136" class="#{pr.categoria} gradeTextWide">
									#{pr.categoria}<span id="yui-gen206" class="#{pr.claseNumero}">#{pr.grado}</span>
								</span><br /> 
							</div>
						</div>
						<div id="backCalif" class="#{pr.visibleDescCalif}"></div>
						<div id="contCalif" class="#{pr.visibleDescCalif}">
							Calificación <b>kubo</b>
						</div>
						<div id="tipo" class="#{pr.color}"></div>
						<div id="contTipo">
							<div id="imgTipo">
								<p:graphicImage width="32px" height="32px" url="../resources/img/#{pr.tipoImg}"></p:graphicImage>
							</div>
							<div id="titTipo">
								#{pr.tipoTit}
							</div>
						</div>
						<div id="insignia" class="#{pr.display}">
							<p:commandLink action="curador"
											actionListener="#{curatedController.iniciaAlianza}"
							>
								<p:graphicImage width="32px" height="32px" url="../resources/img/#{pr.urlAlianza}" style="border: 0px;"></p:graphicImage>
								<f:attribute name="curador" value="#{pr.rutaAlianza}" />
							</p:commandLink>
						</div>
						<div id="descripcion">
							<div id="descCont">
								<div id="descTitulo">#{pr.descTitulo}</div>
								<div id="descAutor">Autor: <span>#{pr.descAutor}</span></div>
								<div id="descDesc">#{pr.descDescripcion}</div>
							</div>
						</div>
						<div id="descPorc"><p:progressBar value="50" label=" "></p:progressBar></div>
						<div id="datosProy">
							<table class="datosProy">
								<tr>
									<td class="columnDatosProy"><b>50%</b><br />Fondeado</td>
									<td class="columnDatosProy"><b>$#{pr.montoRestante}</b><br />Requerido</td>
									<td class="columnDatosProy"><b>#{pr.diasRestante}</b><br />Días Restantes</td>
								</tr>
							</table>
						</div>
					</div>
				</div>
			</h:panelGroup>
		</ui:repeat>
		</h:form>
		</div>
		
		<div id="footer" style="width: 100%; magin-right: 0px;">
			<ui:include src="templates/footerContenido.xhtml" /> 
		</div>
		
		<input type="hidden" id="menuSelect" value="" />
		
	</h:body>
</html>