<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"				
				xmlns:p  = "http://primefaces.org/ui">
				
	<div class = "dvContent stlCntNumExt" >
		<div class = "dvLabel">
			<h:outputLabel styleClass = "labelsStl" value = "#{msg.ExtNumber_text}" />
		</div>
		
	
		<div class = "dvElement" >
		
			<h:inputText id = "address_number" immediate  = "true" 
						 tabindex   = "30"
						 onkeypress = "return isNumberKey(event);" 
						 value = "#{employment.addressbean.address_number}" 
						 onfocus = "ActividadEconomica.Domicilio.init_focus_date(4);"
						 styleClass = "elementAddresMin mapClass address_number">
						 
				<p:ajax event = "blur" 
						onstart    = "return validateNumAdrress('uiemp\:#{status.index}\:address_number')"
						oncomplete = "ActividadEconomica.Domicilio.input_text_oncomplete(xhr, status, args);" 
						listener   = "#{addMyLoan.domicilio.init_input_text}"/>
			</h:inputText>
			
			<h:inputHidden value = "#{employment.addressbean.address.address_number}"/>
		</div>
	</div>
</ui:composition>		