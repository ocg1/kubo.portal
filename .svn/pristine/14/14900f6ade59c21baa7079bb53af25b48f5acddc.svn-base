<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
				
	<div class = "field" style = "display:none">
		<div class = "dvLabel" style = "width: 350px;">
			<h:outputLabel styleClass = "labelsStl" value = "¿Cuántas personas dependen económicamente de ti? *"/>
		</div>
		
		
		
		<div  class="dvElementAddres1" >
			<h:inputText id = "numDepen" rendered = "false"
						 onkeypress = "return isNumberKey(event);" 
						 value      = "#{addMyLoan.naturalperson.dependants_number}" 
						 styleClass = "elementAddres1 hideIFE validatorClass hs_class">
				<p:ajax event    = "blur" 
						onstart  = "hideIFEjs()" 
						listener = "#{addMyLoan.listener_dependientes_economicos}"/>
			</h:inputText>
		</div>
	</div>				
</ui:composition>				