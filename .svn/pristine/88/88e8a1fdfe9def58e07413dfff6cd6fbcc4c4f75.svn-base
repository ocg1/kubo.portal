<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
			     xmlns:p  = "http://primefaces.org/ui">
			     
	<div class = "dvContent">
		<h:inputHidden id = "address" styleClass = "elementAddres1" />
	</div>				 
	
	<div class = "field">
		<div class = "dvLabel">
			<h:outputLabel styleClass = "labelsStl" value = "#{msg.Street_text}" />
		</div>
		
	
		
		<div class = "dvElementAddres1">
			<h:inputText id = "street" immediate = "true"
						 onfocus = "Domicilio.init_focus_date(1);"
						 value   = "#{basicData.domicilio.address.street}"  
						 styleClass = "elementAddres1 validatorClass mapClass street hs_class" >
						 
				<p:ajax event = "blur"  
						oncomplete = "Domicilio.street_on_complete(xhr, status, args);"
						listener   = "#{basicData.domicilio.init_street}"/>
			</h:inputText>		
			<p:watermark for="street" value="Escribe tu calle"/>					
		</div>
	</div>
	
</ui:composition>	