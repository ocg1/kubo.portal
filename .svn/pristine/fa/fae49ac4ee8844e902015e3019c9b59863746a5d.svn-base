<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
		<div style="display: none;"  >
			<div id="displayQuestion">
				<h:panelGroup id="pnlQuestionsC">
					<h:panelGrid columns="2" style="margin-bottom:10px" cellpadding="5">  
		        			<p:selectManyCheckbox id="questionss" value="#{summaryRequest.selectedUnrealizedQuestions}" layout="pageDirection">  
		            			<f:selectItems value="#{summaryRequest.listUnrealizedQuestions}" var="var" itemLabel="#{var.question}" itemValue="#{var.proyectQuestionPK.proyect_question_id}" />
		            			<p:ajax event="change"/>
		        			</p:selectManyCheckbox>
	        		</h:panelGrid>
        		</h:panelGroup>
        		<div style="text-align: center; margin-bottom: 10px;">
        		<p:commandButton value="Preguntar" actionListener="#{summaryRequest.setQuestionsinForum}" oncomplete="$.fancybox.close();" />
        		</div>
        		<div style="display: none;">
					<p:commandLink id="questActUpdA" update="pnlQuestionsC"/>
				</div>
			</div>
		</div>				
</ui:composition>				