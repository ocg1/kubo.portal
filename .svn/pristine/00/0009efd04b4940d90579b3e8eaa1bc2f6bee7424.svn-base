<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">	
					
					
	<div id = "preguntas_error_msg" style = "display: none;">
		<p style = "color: red;">No se permiten preguntas duplicadas</p>
	</div>					
						
	<div id = "guardar_cambios_preguntas" class="btn_aceptar_cancelar" style="display:none">
	

		
		<div 	class	= "callActionMedium btnGris" onclick = "cancelar_cambio_pregunta()">
			<h:outputText value = "Cancelar" />
		</div>
		
				<p:commandLink	immediate      = "true" 							
						actionListener = "#{perfilController.guardar_cambio_pregunta}"
						oncomplete     = "callback_guardar_cambios(xhr, status, args);"
						value 		   = "Cambiar"
						styleClass	   = "callActionMedium btnNaranja"
						>
						
		
			
		</p:commandLink>
	</div>	
</ui:composition>			
			