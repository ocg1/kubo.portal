<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
				
	<h:panelGroup rendered = "#{summaryRequest.editor_domicilio.coincidencias_NIVEL_1 gt 0 and sessionBean.area == 'M'}">
			
		<p:commandLink id = "alarma_coincidencias_NIVEL_1" onclick = "init_listener_lista_coincidencias('NIVEL_1', 'CASA'); ">
	
			<img title  = "#{summaryRequest.editor_domicilio.alarma_NIVEL_1}"
				 src    = "../resources/img/warning01.png" 
				 width  = '20px' 
				 height = '20px'			 
				 style  = 'margin-left: 10px; border:0; cursor: pointer;'/>
				
		</p:commandLink>
													
	</h:panelGroup>	
	
	<p:commandLink id = "listener_lista_coincidencias_NIVEL_1"
				   immediate = "true" 
		 		   actionListener = "#{summaryRequest.editor_domicilio.listener_lista_coincidencias}"
		 		   onstart    = "displayMessageProcessing('msgprocessing',false);"
		 		   oncomplete = "listener_lista_coincidencias(xhr, status, args);"
		 		   update     = "panel_lista_coincidencias"
		 		   style      = "display: none"/>			
</ui:composition>	