<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
		
	<div id = "edicion_meta_INFO" style = "display:none">
		<h:panelGrid columns     = "2" 
					 cellpadding = "5"			     
				     style       = "margin-bottom:10px">
				     
	        <p:selectBooleanCheckbox id = "datos_documentos_OK" 
	        						 disabled   = "true"
	        						 value      = "#{summaryRequest.notas.datos_documentos_OK}" 
	        						 styleClass = "visita_domiciliaria_INFO">
	        	<p:ajax listener = "#{summaryRequest.notas.listener_datos_documentos_OK}"/>
	        </p:selectBooleanCheckbox>
	        
	        <h:outputText value = "Los datos y los documentos son correctos" />	        
	 
	 		<p:selectBooleanCheckbox id = "domicilio_OK"
	 								 disabled   = "true"
	 								 value      = "#{summaryRequest.notas.domicilio_OK}" 
	 								 styleClass = "visita_domiciliaria_INFO">
	 			<p:ajax listener = "#{summaryRequest.notas.listener_domicilio_OK}"/>
	 		</p:selectBooleanCheckbox>
	 		
	        <h:outputText value = "El domicilio es correcto" />	        
		</h:panelGrid>	
	</div>
</ui:composition>