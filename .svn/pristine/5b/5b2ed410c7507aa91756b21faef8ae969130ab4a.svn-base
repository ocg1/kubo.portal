<ui:composition 
	xmlns    = "http://www.w3.org/1999/xhtml"
	xmlns:h  = "http://java.sun.com/jsf/html"
	xmlns:ui = "http://java.sun.com/jsf/facelets"
	xmlns:f  = "http://java.sun.com/jsf/core"
	xmlns:p  = "http://primefaces.org/ui">	
	<div class = "menu_subtitulo">
		<h:outputLabel>Tipos de cr√©ditos</h:outputLabel>
	</div>

	<div class = "main_menu_items">
		<ui:repeat var = "m" value = "#{menuController.menusTipos}">			
			<p:commandLink actionListener = "#{menuController.cambiaPagina}"						   
						   immediate      = "true" 
						   update         = ":form_Prin:contPage">
				<div id      = "dv#{m.direccion}"
					 class   = "west_menu_item"  
					 onclick = "selecciona_menu_item('dv#{m.direccion}');">
					<h:outputLabel value = "#{m.nombre}" />
					<f:attribute name = "capitulo" value="#{m.direccion}" />
				</div>
			</p:commandLink>			
		</ui:repeat>
	</div>
</ui:composition>	