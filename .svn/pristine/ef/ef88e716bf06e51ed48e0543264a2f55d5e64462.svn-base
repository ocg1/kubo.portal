package mx.com.kubo.bean;

import mx.com.kubo.model.Income;

public class IncomeBean {
	
	private Integer income_type_id;
	private Double ammount;
	private String name;
	private Income income;
	private String ammount_modified;
	
	public IncomeBean(){
		
	}
	public IncomeBean(Integer income_type_id,Double ammount,String name) {
		this.income_type_id=income_type_id;
		this.ammount=ammount;
		this.name=name;
	}
	
	public Integer getIncome_type_id() {
		return income_type_id;
	}
	public Double getAmmount() {
		return ammount;
	}

	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setIncome_type_id(Integer income_type_id) {
		this.income_type_id = income_type_id;
	}
	public void setAmmount(Double ammount) {
		this.ammount = ammount;
	}
	public Income getIncome() {
		return income;
	}
	public void setIncome(Income income) {
		this.income = income;
	}
	public String getAmmount_modified() {
		return ammount_modified;
	}
	public void setAmmount_modified(String ammount_modified) {
		this.ammount_modified = ammount_modified;
	}
	
}
