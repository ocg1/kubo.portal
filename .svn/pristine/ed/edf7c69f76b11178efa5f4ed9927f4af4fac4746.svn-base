<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
	<li>
		<label class="etiqueta">Cuenta: </label>
		
			<div class="campo">
				<div class="styled-select select_border">
					<h:selectOneMenu immediate  = "true"
								     tabindex   = "1" 
								     styleClass = "elementStlSelAddress" 
								     value      = "#{movimientosInversionista.cancelacion.cuenta}">  
								     
				        <f:selectItem itemValue = "" itemLabel = "Todas las cuentas" />
				        
				        <f:selectItems value = "#{movimientosInversionista.cancelacion.lista_cuentas}" 
				                       var   = "account" 
				                       itemLabel = "#{account.accountName}" 
				                       itemValue = "#{account.account}"/>
				                       
				        <p:ajax event = "change" 
				        	    listener   = "#{movimientosInversionista.cancelacion.listener_cuenta}" 
				        	    oncomplete = "callback_cuenta(xhr, status, args);"/>    
				    </h:selectOneMenu>
				</div>
			</div>	
		
	</li>				
</ui:composition>	