<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
				 xmlns:p  = "http://primefaces.org/ui">	

	<div style = "clear:both; margin: 5px 0;">			
		<h:commandLink>
			<p:ajax event = "click" update = "editor-nombre" immediate="true"
			        onstart = "displayMessageProcessing('msgprocessing',false);"			  
				    oncomplete = "guardar_edicion_nombre(xhr, status, args);"
				    listener = "#{summaryRequest.editor_nombre.listener_guardar_cambios}"/>
					   		   
			<div class = "buttonPR" 
			     style = "width:100px; 
			     		  font-size: 0.9em; 
			     		  float: left; 
			     		  margin: 0 5px 0 0;" >Guardar</div>		
		</h:commandLink>	
			
		<h:commandLink>			
					   
			<p:ajax event = "click" update = "editor-nombre" immediate="true"					   
					onstart        = "displayMessageProcessing('msgprocessing',false);"
					oncomplete     = "cancelar_edicion_nombre(xhr, status, args);"
					listener = "#{summaryRequest.editor_nombre.listener_cancelar}"/>
					 					 	
				<div class   = "buttonPR"  
					 style   = "margin: 0; 
					 			margin-left: 135px;
					 			width: 100px;
					 			font-size:0.9em;
					 			float: left;">Cancelar</div>
		 </h:commandLink>
	</div>
</ui:composition>		