			</div>
		<h:form id="form_Prin">	
		
		<div id="titulo" style="width: 450px; color:#000000;border: solid 0px #FF0000;position: absolute; top: 123px;left: 400px;">Compara tus préstamos</div>
		<h:panelGroup rendered="#{sessionBean.prospectus_id==null}">
			<div id="comparaLnk" style=" margin-bottom: 15px !important; height: auto !important; overflow: auto !important;">
				<div style="float: left; margin-bottom: 10px !important; margin-top: 10px !important;color:#333333;">
					&nbsp;
					
					Para obtener más beneficios 
				</div>
				<div style="float: left; margin-bottom: 0px; margin-left: 10px; margin-top: 5px;padding-bottom: 0px; padding-top: 0px;">
					<div class="buttonPR"  style="margin-bottom: 0px !important; margin-top: 0px !important;">
						<p:commandLink action="preregistro" id="registro" styleClass="actionSim">
							Regístrate
						</p:commandLink> 
					</div>
				</div>
			</div>
		</h:panelGroup>
		
			<div id="conCompara">
				<div id="compara"></div>
			</div>
			<div id="legalString">
				
			</div>		
			<div id="nav">
				<div id="back"><p:graphicImage url="../resources/img/Symbol-Back-32.png" ></p:graphicImage>&nbsp;atrás</div>
				<div id="next">siguiente&nbsp;<p:graphicImage url="../resources/img/Symbol-Next-32.png" ></p:graphicImage></div>
			</div>
			
		
		<div id="contMsj">
			
			<div id="mensaje4" class="buttonPR">
				<p:commandLink action="simulator" id="simulador" styleClass="actionSim">
					Realiza más simulaciones
				</p:commandLink> 
			</div>
		
			<div id="mensaje2" class="buttonPR">Realiza otra comparación<br></br></div>
			<!-- <div id="mensaje3" class="buttonPR">Enviamos tus comparaciones por mail</div> -->
			
		</div>
		
		<input type="hidden" id="numero" value="1"></input>
		</h:form>
		<div id="header" style="width: 100%; magin-right: 0px; margin-bottom: 0px;">
			<ui:include src="templates/footerContenido.xhtml" />
		</div>
		
		<div style="display: none;">
			<h:form id="frmSimHide">
				<p:inputText value="#{comparison.cuotaInStr}" id="cuota">
					<p:ajax event="blur"></p:ajax>
				</p:inputText>
				<p:inputText value="#{comparison.montoInStr}" id="monto">
					<p:ajax event="blur"></p:ajax>
				</p:inputText>
				<p:inputText value="#{comparison.plazo}" id="plazo">
					<p:ajax event="blur"></p:ajax>
				</p:inputText>
				<p:inputText value="#{comparison.frequency}" id="freq">
					<p:ajax event="blur"></p:ajax>
				</p:inputText>
				<p:inputText value="#{comparison.nameInst}" id="nameInst">
					<p:ajax event="blur"></p:ajax>
				</p:inputText>
				
				<p:inputText id="comparaAction">
					<p:ajax event="blur" listener="#{comparison.myComparison}" oncomplete="muestraComparacion();" update="resultado" ></p:ajax>
				</p:inputText>
				
				<p:inputText value="#{comparison.cadenaResultado}" id="resultado" />
				
				<input type="hidden" id ="numeroSel" value=""></input>
			</h:form>
		</div>
		
	<script>
		creaComparacion();
	</script>
	
