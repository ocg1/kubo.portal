@CHARSET "UTF-8";

div#container_vivienda_TOKEN
{ 
	cursor: pointer;
	background-color: white;
	
	border:0px solid black;
}
div#container_vivienda_TOKEN p{
	color: #439539; 
}

div#container_vivienda_TOKEN:hover
{		
	cursor: pointer;

	color: #ffffff;
}
div#container_vivienda_TOKEN p:hover
{	
color:#8FC450 !important
}
div#container_editar_vivienda_TOKEN
{
	display: none;
	font-size:16px !important;
	margin: 20px 0 0 0;
	color: #666;
	width: 400px;
}

#container_editar_vivienda_TOKEN .dvElementAddres1{
	width:400px;

	margin: 0 0 10px !important;
	padding: 0 !important;
}

#container_editar_vivienda_TOKEN  .dvContent {
	margin:0;
}
#container_editar_vivienda_TOKEN  .dvLabel {
	font-size: 11px !important;

	width: 100%;
	padding: 0;
	margin: 0;
}

#container_editar_vivienda_TOKEN  .styled-select{
	width:90% !important;

}
#container_editar_vivienda_TOKEN  input, #container_editar_vivienda_TOKEN  select, #container_editar_vivienda_TOKEN  textarea{
	font-size: 16px !important;	
	color:#666;
}

#container_editar_vivienda_TOKEN  .dvElement {
	padding:0;
}
#container_editar_vivienda_TOKEN .link_editar_curp {
    margin:0;
}
#container_editar_vivienda_TOKEN .link_editar_curp p {
    color: #439539 !important;
	font-size:14px;
}

#container_editar_vivienda_TOKEN .stlCntNumExt, #container_editar_vivienda_TOKEN  .stlCntNumInt, #container_editar_vivienda_TOKEN .stlCntNumApple, #container_editar_vivienda_TOKEN .stlCntNumLote {
	float: left !important;
	display: block !important;
	clear: none !important;
	width: 45% !important;
}