package mx.com.kubo.bean;

import java.io.Serializable;

public class Proyecto implements Serializable{
	private String urlImg;
	private String tipoImg;
	private String tipoTit;
	private String urlAlianza;
	private String descTitulo;
	private String descAutor;
	private String descDescripcion;
	private String montoRestante;
	private String diasRestante;
	private String color;
	private String categoria;
	private String grado;
	private String claseNumero;
	private String display;
	private String rutaAlianza;
	private String visibleDescCalif;
	
	public Proyecto() {
		super();
	}

	public String getUrlImg() {
		return urlImg;
	}

	public void setUrlImg(String urlImg) {
		this.urlImg = urlImg;
	}

	public String getTipoImg() {
		return tipoImg;
	}

	public void setTipoImg(String tipoImg) {
		this.tipoImg = tipoImg;
	}

	public String getTipoTit() {
		return tipoTit;
	}

	public void setTipoTit(String tipoTit) {
		this.tipoTit = tipoTit;
	}

	public String getUrlAlianza() {
		return urlAlianza;
	}

	public void setUrlAlianza(String urlAlianza) {
		this.urlAlianza = urlAlianza;
	}

	public String getDescTitulo() {
		return descTitulo;
	}

	public void setDescTitulo(String descTitulo) {
		this.descTitulo = descTitulo;
	}

	public String getDescAutor() {
		return descAutor;
	}

	public void setDescAutor(String descAutor) {
		this.descAutor = descAutor;
	}

	public String getDescDescripcion() {
		return descDescripcion;
	}

	public void setDescDescripcion(String descDescripcion) {
		this.descDescripcion = descDescripcion;
	}

	public String getMontoRestante() {
		return montoRestante;
	}

	public void setMontoRestante(String montoRestante) {
		this.montoRestante = montoRestante;
	}

	public String getDiasRestante() {
		return diasRestante;
	}

	public void setDiasRestante(String diasRestante) {
		this.diasRestante = diasRestante;
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}

	public String getCategoria() {
		return categoria;
	}

	public void setCategoria(String categoria) {
		this.categoria = categoria;
	}

	public String getGrado() {
		return grado;
	}

	public void setGrado(String grado) {
		this.grado = grado;
	}
	public String getClaseNumero(){
		return "gradeNumber"+getCategoria();
	}
	public void setClaseNumero(String claseNumero){
		this.claseNumero = claseNumero;
	}

	public String getDisplay() {
		return display;
	}

	public void setDisplay(String display) {
		this.display = display;
	}

	public String getRutaAlianza() {
		return rutaAlianza;
	}

	public void setRutaAlianza(String rutaAlianza) {
		this.rutaAlianza = rutaAlianza;
	}

	public String getVisibleDescCalif() {
		return this.visibleDescCalif;
	}

	public void setVisibleDescCalif(String visibleDescCalif) {
		this.visibleDescCalif = visibleDescCalif;
	}
	
	
}
