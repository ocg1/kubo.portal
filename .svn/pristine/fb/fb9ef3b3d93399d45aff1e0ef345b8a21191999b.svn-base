<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:p  = "http://primefaces.org/ui">
				
	<div class = "left">
		<p:commandLink immediate      = "true"
					   rendered       = "#{summaryRequest.actividad_economica.change_control.hasChange}" 										
					   actionListener = "#{summaryRequest.setListChanges(summaryRequest.actividad_economica.change_control)}" 
					   oncomplete     = "showPopUpChanges()" 
					   style          = "cursor:pointer;">
					   
			<h:graphicImage url      = "../resources/img/change-icon.png"  
							rendered = "#{summaryRequest.actividad_economica.change_control.hasChange}" 
							
							styleClass="change_icon">
			</h:graphicImage>
			
		</p:commandLink>
	</div>	
</ui:composition>	