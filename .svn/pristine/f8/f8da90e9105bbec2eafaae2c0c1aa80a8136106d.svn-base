<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
			     xmlns:f  = "http://java.sun.com/jsf/core"
				 xmlns:p  = "http://primefaces.org/ui">

	<div class="field">
		<div class="dvLabel">
			<h:outputLabel styleClass="labelsStl" value="#{msg.Gender_text} "/>
		</div>
					<fieldset class="conjuntoChecks">
				   <div class="btnN dosBtnChecks"  data-opcion="1">
			        	<span>	Hombre</span>
			       </div>
			       <div class="btnN dosBtnChecks btnR"  data-opcion="2">
			        	<span>	 Mujer</span>
			       </div>
	        		
		
					<h:selectOneRadio  value="#{benefic.gender_id}"  styleClass="elementStlLbl validatorClass requiredVerify ocultaSelect">
					    <f:selectItem itemValue="1" itemLabel="Masculino"/>
					    <f:selectItem itemValue="2" itemLabel="Femenino"/>		
					    <p:ajax event="click" listener="#{ibeneficiaries.beneficiario.persist(benefic)}" oncomplete="conjuntoCombos();" />					    
					</h:selectOneRadio>	
			</fieldset>					

	</div>			
</ui:composition>			