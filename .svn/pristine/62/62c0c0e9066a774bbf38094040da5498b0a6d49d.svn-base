<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
				 xmlns:f  = "http://java.sun.com/jsf/core"
				 xmlns:p  = "http://primefaces.org/ui">	

	<h:panelGroup id = "show-ischange-data-ife">
			<p:commandLink rendered  = "#{summaryRequest.editor_identification.changeDataIFE.hasChange}" 
					       immediate = "true" 
						   oncomplete     = "showPopUpChanges();"
						   actionListener = "#{summaryRequest.setListChanges(summaryRequest.editor_identification.changeDataIFE)}" 								    
						   style = "cursor:pointer;">
				<h:graphicImage url = "../resources/img/change-icon.png" />
			</p:commandLink>
	</h:panelGroup>
</ui:composition>			