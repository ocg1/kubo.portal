package mx.com.kubo.referencia_pago;

import java.util.ResourceBundle;

public class ReferenciaPagoEMO
{
	private ResourceBundle recurso;
	
	private String url_logo;
	private String razon_social;		
	private String no_referencia;
	private String no_cuenta;
	private String CLABE;
	private String cliente;
	private String pago;
	
	private String no_empresa;
	private String sucursal;
	private String pago_servicio;
	
	private Nota nota;
	
	public ReferenciaPagoEMO()
	{
		recurso = ResourceBundle.getBundle("Message.MessageResourceBundle");
		
		razon_social = recurso.getString("razon_social_kubo_sofipo");
	}
	
	public String getUrl_logo() {
		return url_logo;
	}
	
	public void setUrl_logo(String url_logo) {
		this.url_logo = url_logo;
	}

	public String getRazon_social() {
		return razon_social;
	}

	public String getNo_empresa() {
		return no_empresa;
	}

	public void setNo_empresa(String no_empresa) {
		this.no_empresa = no_empresa;
	}
	
	public String getSucursal() {
		return sucursal;
	}

	public void setSucursal(String sucursal) {
		this.sucursal = sucursal;
	}

	public String getPago_servicio() {
		return pago_servicio;
	}

	public void setPago_servicio(String pago_servicio) {
		this.pago_servicio = pago_servicio;
	}

	public String getNo_referencia() {
		return no_referencia;
	}

	public void setNo_referencia(String no_referencia) {
		this.no_referencia = no_referencia;
	}

	public String getPago() {
		return pago;
	}

	public void setPago(String pago) {
		this.pago = pago;
	}
	
	public String getNo_cuenta() {
		return no_cuenta;
	}

	public void setNo_cuenta(String no_cuenta) {
		this.no_cuenta = no_cuenta;
	}

	public String getCLABE() {
		return CLABE;
	}

	public void setCLABE(String cLABE) {
		CLABE = cLABE;
	}

	public String getCliente() {
		return cliente;
	}

	public void setCliente(String cliente) {
		this.cliente = cliente;
	}

	public final Nota getNota() 
	{
		return nota;
	}

	public final void setNota(Nota nota) 
	{
		this.nota = nota;
	}
}
