<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
			     xmlns:p  = "http://primefaces.org/ui">

	<div class = "stlCntNumInt" >
		<div class = "dvLabel stlNumInt">
			<h:outputLabel styleClass = "labelsStl" value = "#{msg.IntNumber_text}" />
		</div>
		
	

			<h:inputText id = "numInt" size = "4" immediate = "true" maxlength = "10"
						 onfocus = "Domicilio.init_focus_date(1);"
						 value = "#{basicData.domicilio.address.apt_number}" 
						 styleClass = "elementAddresMin">
						 
				<p:ajax event = "blur"
						oncomplete = "Domicilio.apt_number_on_complete(xhr, status, args);"  
						listener   = "#{basicData.domicilio.init_apt_number}"/>
			</h:inputText>
	
	</div>				
</ui:composition>					