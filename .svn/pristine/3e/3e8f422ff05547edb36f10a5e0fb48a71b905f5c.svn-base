<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
				

					<div class="field" >
	
		<div class = "dvLabel">
			<h:outputLabel styleClass = "labelsStl" value = "#{msg.citizenship} "/>
		</div>
		
	
		

			<div class = "selectNuevo">
			
				<h:selectOneMenu id = "nacionalidad" immediate = "true"
								 value    = "#{basicData.naturalPerson.citizenship}" 
								 disabled = "#{navigationBean.disabledComponent('nacionalidad')}" 							      
							     onfocus  = "DatosPersonales.init_focus_date(3);"
							     styleClass = "elementStlSelAddress requiredVerify validatorClass hs_class">
							     
								<f:selectItem itemLabel = ""  itemValue = "" />

								<f:selectItem itemValue = "1" itemLabel = "Mexicana"/>
							    <f:selectItem itemValue = "0" itemLabel = "Extranjero"/>
							
				    
					<p:ajax event      = "change" update = "nacmex nacext panel-rfc-curp domicilio_title domicilio_pais_origen domicilioNacional"
							listener   = "#{basicData.citizenship.init_change}"  
							oncomplete = "DatosPersonales.citizenship_on_complete(xhr, status, args);" />
				</h:selectOneMenu>																
			</div>			
		
	</div>				
</ui:composition>				