<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
				 xmlns:p  = "http://primefaces.org/ui">	
				 
	<div class = "dvContent">
		<div class = "dvLabel" style = "margin: 0;">
			<h:outputLabel value = "#{msg.Zipcode_text}" styleClass = "editor-domicilio label"/>
		</div>		
		
		<p/>	
		
		<div id = "vivienda_codigo_postal">					
			<h:inputText immediate  = "true"
					     onkeypress = "return isNumberKey(event);"  
					     value      = "#{summaryRequest.editor_domicilio.codigo_postal}" 
					     maxlength  = "5"  
					     styleClass = "elementAddres1 validatorClass zipcode editor-domicilio input-text">
					     
				<p:ajax event      = "blur" 
						onstart    = "return validateZipCode('vivienda_codigo_postal');" 
						oncomplete = "callback_listener_codigo_postal(xhr, status, args);" 
						listener   = "#{summaryRequest.editor_domicilio.listener_codigo_postal}"/>
						
			</h:inputText>	
		</div>
	</div>				 
</ui:composition>				 