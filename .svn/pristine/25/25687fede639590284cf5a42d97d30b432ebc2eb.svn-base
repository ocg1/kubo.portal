<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">		
					
	<div class = "field">
		<div class = "dvLabel">
			<h:outputLabel styleClass = "labelsStl" value = "#{msg.Neighborhood_text}" />
		</div>
		

		
		<div class = "dvElementAddres1">
			<div class = "selectNuevo">
				<h:selectOneMenu id = "coloniaEmp" tabindex="50" immediate = "true"
								 value = "#{business.addressbean.neighborhood_id}" 
								 onfocus = "ActividadEconomica.Domicilio.init_focus_date(3);"
								 styleClass = "elementStlSelAddress validatorClass mapClass neighborhood hs_class" 
								 style = "height: 30px;">
								 
					<f:selectItem itemLabel = "" itemValue = "" />
					
					<f:selectItems value = "#{business.addressbean.listNeighborhood}" 
								   var = "asents" 
								   itemLabel = "#{asents.name}" 
								   itemValue = "#{asents.neighborhoodCatPK.neighborhood_id}" />
								   
				<p:ajax event = "change"
						oncomplete = "ActividadEconomica.Domicilio.neighborhood_id_oncomplete(xhr, status, args);" 
					    listener = "#{addMyLoan.domicilio_negocio.init_neighborhood_id}"/>
				</h:selectOneMenu>		
			</div>						
		</div>
	</div>
</ui:composition>			