@CHARSET "UTF-8";

@media screen and (max-width: 1250px) {
		
		#sobresalientes{
			display: block !important;
			border: solid 0px #FF0000 !important;
		}
		
		#contenido{
			width: 45% !important;
			border: solid 0px #FF0000 !important;
			/* top: 80px !important; */
		}
		
		
		#sobresalientes_med{
			display: none !important;
			position: fixed;
			right: 0px;
			top: 200px;
			border: solid 0px #00FF00 !important;;
		}
		
		#conPageSim{
			width: 68% !important;
			float: left;
		}
		
		#indice{
			width: 23% !important;
		}
		
		#content{
			width: 99%;
			border: solid 0px #FF0000 !important;
			min-width: 50px;
		}

		.tituloCnt {
			font-size: 1.87em !important;
			line-height: 30px;		
		}
		
	}
	
	
	@media screen and (max-width: 850px) {
	
		#sobresalientes{
			display: none !important;
			border: solid 0px #FF0000 !important;
		}
		#conPageSim{
			width: 67% !important;
			border: solid 0px #FF0000 !important;
			float: right;
		}
		#contenido{
			width: 60% !important;
			border: solid 0px #FF0000 !important;
		}
		#indice{
			width: 30% !important;
		}
		.tituloCnt {
			margin-bottom: 25px !important;	
		}
	}
