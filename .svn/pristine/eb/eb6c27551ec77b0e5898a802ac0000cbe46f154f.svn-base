<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">
				
	<p style = "margin:0 auto; border:0px solid black" >Seleccione prioridad de la nota</p>
	
	<div class = "main_selectmenu_prioridad" style = "width: 344px !important; border: solid 0px; height: 28px !important;">															
		<h:selectOneMenu id         = "select-note-priority"  
						 value      = "#{summaryRequest.notas.tipo_prioridad_SELECTED}" 
						 style 		= "width: 360px !important;">
						 
				<f:selectItem itemValue = ""  itemLabel = ""/>	
					
				<f:selectItems  value     = "#{summaryRequest.notas.lstNotePrioriType}" 
								var       = "notePriority" 
								itemLabel = "#{notePriority.name}" 
								itemValue = "#{notePriority.priorityTypePK.priority_type_id}"/>	
																		
				<p:ajax event    = "change" 
				        listener = "#{summaryRequest.notas.listener_tipo_prioridad_SELECTED}"/> 		
																	
		</h:selectOneMenu>																							
	</div>		
</ui:composition>	