@CHARSET "UTF-8";

.contAvLegal{
	width: 100%;
	border: solid 0px #bbbbbb;	
	margin-left: auto;
	margin-right: auto;
	margin-top:20px;
	height: auto;
	overflow: auto;
}
.contMenuAL{
	width: 230px;
	margin-top: 20px;
	float: left;
}

.contContenido{
	width: 60%;
	height: auto;
	border: solid 0px #cccccc;
	float: left;	
	padding: 20px;
	overflow: auto;
}
.menuAL{
	font-size: 1.3em;
	color: #666666;
	padding-top: 6px;
	padding-bottom: 6px;
	background-color: #FFFFFF;
}
.menuAL:hover{
	cursor: pointer;
}
.borderT{
	border-top: solid 1px #cccccc;	
}
.borderB{
	border-bottom: solid 1px #cccccc;	
}
.lstMenu ul {
    list-style: none;
    padding:0;
    margin:0;
}

.lstMenu li { 
    padding-left: 1em; 
    /*text-indent: -.7em;*/
}
.lstMenu li:before {
    content: "";
    color: #C1D82F; /* or whatever color you prefer */
    
}
.selected{
	color:  #439539;
}
.titleAvLe{
	color: #000000;
	font-size: 1.8em;
	color:  #439539;
}

.titleAvMed{
	color: #333333;
	font-size: 1.6em;
}

.txtCont{
	font-size: 1.2em;
	text-align: justify;
	margin-top: 20px; 
}

.numTxtCnt{
	color: #439539;
	margin-top: 10px;
	margin-bottom: 5px;
}

.socialContent{
	background-image: url("../img/fondo.png");
	background-repeat: repeat;
	width: 205px; 
	height: 100px; 
	border: solid 1px #439539;
	position: absolute; 
	right: 0px; 
	 top: -10px; 
	/* bottom: 0px; */
	padding-bottom: 25px;
	padding-top: 25px;
}
.disabled{
	display: none;
}

.lstItem{
	color: #439539;
	font-weight: bold;
}
li.lstAlpha{
	list-style-type: lower-alpha;
}