<ui:composition  xmlns    = "http://www.w3.org/1999/xhtml"
				 xmlns:ui = "http://java.sun.com/jsf/facelets"
			     xmlns:h  = "http://java.sun.com/jsf/html"
				 xmlns:f  = "http://java.sun.com/jsf/core"
				 xmlns:p  = "http://primefaces.org/ui">
	
	<div id = "msg_error_edicion_vivienda" style = "display: none; padding:5px 0">
		<p style = "color: red"/>
	</div>
	
	<div  class="dos_botones">			
		<p:commandLink immediate      = "true"
					   onstart        = "return validar_formulario_edicion_vivienda();"
					   oncomplete     = "guardar_edicion_vivienda(xhr, status, args);"
					   update         = "vivienda"
					   actionListener = "#{summaryRequest.editor_domicilio.listener_guardar_edicion}"
					   styleClass 	  = "buttonPR"
					   value		  = "Guardar">
					
		</p:commandLink>	
											 	
	<div class   = "buttonPR"  
		 onclick = "cancelar_edicion_vivienda();"
		  >
		  Cancelar</div>
	</div>				 
</ui:composition>				 