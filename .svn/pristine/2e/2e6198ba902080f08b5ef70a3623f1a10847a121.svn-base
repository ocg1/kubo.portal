<ui:composition 
	xmlns    = "http://www.w3.org/1999/xhtml"
	xmlns:h  = "http://java.sun.com/jsf/html"
	xmlns:ui = "http://java.sun.com/jsf/facelets"
	xmlns:f  = "http://java.sun.com/jsf/core"
	xmlns:p  = "http://primefaces.org/ui">	
	
	<h:panelGroup rendered = "false" >	
		<div id = "dvPreg">
			<div id = "pnlPreg">
				<h:outputLabel value = "Preguntas Frecuentes" styleClass="subtituloMenu"/>
				<div>
					<hr class = "hrCnt" style="width: 100%; left: 0px;"></hr>
				</div>
				<div id="preguntas" >
					<ui:repeat var="q" value="#{navegaController.preguntas}">
						<div class="gridQuest">
							<!-- <div class="linksQuest"> -->
							<a class="iframe2"  href="#{q.dirPreg}">#{q.pregunta}</a>
						</div>
					</ui:repeat>
				</div>
				<div style=" height: auto; overflow: auto; float: right;">
					<p:graphicImage url="../resources/img/dudas_02.png"></p:graphicImage>
				</div>
			</div>
		</div>
	
		<div id = "callBook">
			<div id="descBook">
				<b>Â¿Dudas?</b><br />Da click para ingresar a nuestro
			</div>
			
			<div id="butonBook">
				<div id="callToActionBook">
					<div id="callBotBook">
						<p:commandLink action="preguntas" styleClass="link2">
							Manual de ayuda
						</p:commandLink>
					</div>
				</div>
			</div>	
		</div>
	</h:panelGroup>	
</ui:composition>