<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:f  = "http://java.sun.com/jsf/core"
				xmlns:p  = "http://primefaces.org/ui">

	<h:panelGroup id = "lst">	
		<h:panelGroup id = "errorsPage" rendered = "#{addInvestorSafi.errorDisp}">			
			<h:panelGroup rendered = "#{addInvestorSafi.listRequiredMenu != null and addInvestorSafi.listRequiredMenu.size() > 0}">
				<div class="panel_incompleta" style="">
					<div class="clTitle">
							Aún no está completa tu solicitud
					</div>
			<script>
		
				changeUrlParam('revisionCampos', true);
			</script>
					<div>
						<span >
						Para poder finalizar el proceso, por favor completa la información marcada como obligatoria en las siguientes pantallas.
						</span>
					</div>
					
					<ul id = "requiered-info-menu" style="color:#439539;list-style: square outside none;">
						<ui:repeat  var="m" value="#{addInvestorSafi.listRequiredMenu}">
							<li style="margin:5px 0;">
								<p:commandLink immediate = "true"	
											   oncomplete = "closeFancy();"					
											   onstart    = "return displayMessageProcessing('msgprocessing', false);"																	
											   actionListener = "#{navigationBean.cambiaPagina}"	
											   update = ":actualPage :form_Prin:hiddenactios :form_Prin :contSimReg"								
											   style  = "color:#439539;text-decoration:underline;">
									<f:attribute name = "section" value = "#{m.targetItem}::#{m.screenid}::#{m.idItem}" />
									#{m.nameItem}
								</p:commandLink> 								
							</li>
						</ui:repeat>
					</ul>
				</div>
			
										
			</h:panelGroup>		
		</h:panelGroup>
	</h:panelGroup>
</ui:composition>