.container1 .scroll_container {
	overflow: hidden;
	position: relative;
}
.container1 .scroll_absolute {
    position: absolute;
    padding: 0 0px 0  0 !important;
}
.container1 .scroll_vertical_bar {
    position: absolute;
    top: 0;
    right: 0;
    width: 13px;
    height: 100%;
