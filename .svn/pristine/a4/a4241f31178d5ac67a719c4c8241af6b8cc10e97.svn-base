
/*----INICIO interior_lista.html e interior_mosaico.html----*/
.intro, .mosaico h4  {
	overflow: visible;
	width:100%;
	color:#565b59;
	font-size:18px;
	display: block;
}
.lista_completa {
	width: 100%;
	padding: 0 0px 0px 0px;
	display:block;
	float:left;
	margin-bottom: 20px;
}
.disponible p , .lista_completa p {
	line-height: 23px;
}
.lista_completa p span, .disponible p span{
	font-weight: 700;	
}
.lista_completa p span{
	color: #538b39;
	font-size:24px;
}
.productos article {
	float:left;
	display:block;
	width: 48%;
	border:solid 1px #cccccc;
	height: 318px;
	font-size:14px;
	color:#565b59;
	margin: 0 0 40px;
	position:relative;
}		
.productos article h2 {
	background:#538b39;
	color:#fff;
	font-size:20px;
	font-weight:500;
	padding:12px 15px 18px;
	line-height:25px;
	letter-spacing: -1px;
}
.productos article h2 span {
	font-size:40px;
	display:block;
	letter-spacing: -2px;
}
.productos article div {
	width:90%;
	overflow:hidden;
	padding:20px 5%;
}
.productos article div p, .productos article div h4 {
	clear:both;
	width:100%;
	display:block;
	overflow:hidden;
	line-height: 19px;
	font-size: 15px;
}
.productos article div h4 {
	position:absolute;
	z-index:1;
	bottom: 24px;
	right:20px;
}
.productos article div h4 a{
	float:right;
	color:#565b59;
	text-decoration:underline;
	clear:both;
}
.productos article div h5{
	width: 220px;
	clear:both;
	display:block;
	right:20px;
	position:absolute;
	z-index:1;
	bottom:10px;
}
.productos article div .solicita {
	background: #ff8a02;
	color: #fff;
	font-size:14px;
	text-transform:uppercase;
	font-weight: 700;
	padding: 8px 0;
	display:block;
	width: 206px;
	float: right;
	margin:0 0 0 10px;
	text-align: center;
}
.productos article.negocio h2 {
	background: #538b39 url(../images/producto1.png) no-repeat 96% 12px;
	background-size:60px auto;
}		
.productos article.deudas h2 {
	background: #538b39 url(../images/producto2.png) no-repeat 96% 20px;
	background-size:40px auto;
}	
.productos article.casa h2 {
	background: #538b39 url(../images/producto3.png) no-repeat 96% 12px;
	background-size:60px auto;
}		
.productos article.impulsa h2 {
	background: #538b39 url(../images/educacion.png) no-repeat 96% 10px;
	background-size:65px auto;
}	

.productos article.salud h2 {
	background: #538b39 url(../images/salud-01.png) no-repeat 96% 10px;
	background-size:65px auto;
}	

.productos article.imprevistos h2 {
	background: #538b39 url(../images/imprevistos.png) no-repeat 98% 10px;
	background-size:65px auto;
}

.productos article.eventos h2 {
	background: #538b39 url(../images/prestamo-vacaciones.png) no-repeat 98% 10px;
	background-size:65px auto;

}

@media (max-width: 1024px) {
	.productos article {
		width:48%;
	}
	.productos article div {
		padding:20px 20px 0 20px;
	}
}

@media (max-width: 850px) {
	.productos article {
		width: 99.5%;
		height:auto;
	}
 .productos article div h4,	.productos article div h5{
		position:relative;
		bottom:auto;
		right:auto;
		float:right;
		margin:0 0 20px;
	}
	.productos article div h5{
		width: 213px;
	}
}

@media (max-width: 650px) {
	.perfiles_inversion {
		padding:30px 0 0 0;
	}
		
	.productos article {
		margin:0;	
		float: left !important;
	}
	.col_derecha_productos .productos article h2, .productos article  h2 span{
		font-size:17px;
	}
	.col_derecha_productos .productos article h2 {
		background-size:25px 25px;
		background-position: 97% center;
		cursor:pointer;
		padding:7px 5%;
		letter-spacing: 0;
}
	.productos article  h2 span{
		display:inline;
		letter-spacing: 0;
	}
	.productos article div h5{
		width: 232px;
	}
	.productos article  div{
		display:none;
	}
	.productos article div .solicita {
		padding:10px 10px;
	
}
	.prestamo_info h3 {
	    margin: 10px 0 0px !important;
	}
	
	.content.top {
		width:100%;
		margin:0;
	
		padding: 0 0 20px;
	}

}

