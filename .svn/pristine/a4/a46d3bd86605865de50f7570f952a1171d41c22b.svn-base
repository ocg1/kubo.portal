					     onfocus    = "Domicilio.init_focus_date(9);"					   
					     styleClass = "elementAddres1 validatorClass mapClass zipcode hs_class"
					     id="zipCode2"
					     > 
				<p:ajax event     	= "blur"
						oncomplete	= "Domicilio.zipcode_on_complete(xhr, status, args, '#street2');" 
						listener  	= "#{basicData.fiscal.init_zipcode}" />
						
			</h:inputText>
			<p:watermark for="zipCode2" value="Escribe tu cÃ³digo postal"/>	
		</div>
	</div>				 
</ui:composition>		