<ui:composition xmlns    = "http://www.w3.org/1999/xhtml"
				xmlns:ui = "http://java.sun.com/jsf/facelets"
				xmlns:h  = "http://java.sun.com/jsf/html"
				xmlns:p  = "http://primefaces.org/ui">
					
	<h:panelGroup rendered = "#{!summaryRequest.editor_domicilio_ENABLED}">						
		<p>#{summaryRequest.domicilio_actividad.activity_TOKEN}</p>
		
		<h:panelGroup rendered = "#{summaryRequest.nameVisible}">
			<p>Ubicado en #{summaryRequest.domicilio_actividad.address_TOKEN}</p> 
		 </h:panelGroup>
	</h:panelGroup>	
	
	<h:panelGroup rendered = "#{summaryRequest.editor_domicilio_ENABLED}">				
		<div id = "business-address-TOKEN" onclick = "ActividadDomicilio.init();">	
			<p>#{summaryRequest.domicilio_actividad.activity_TOKEN}</p>
			
			<h:panelGroup rendered = "#{summaryRequest.nameVisible}">
				<p>Ubicado en #{summaryRequest.domicilio_actividad.address_TOKEN}</p> 
			 </h:panelGroup>
		</div>
	</h:panelGroup>	
</ui:composition>	