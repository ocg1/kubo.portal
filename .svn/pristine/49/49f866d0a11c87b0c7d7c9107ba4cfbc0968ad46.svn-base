function loadScriptsearch()
{
		$("h2.expand_heading").click(function(event){
			var children=$(this).children();
			
			if(children.hasClass('change')){
					$(this).next(".toggle_container").slideUp("slow");
					children.removeClass("change");
				}
			else{
					$(this).next(".toggle_container").slideDown("slow");
					children.addClass("change");
				}
			
			event.preventDefault();
		});	
		
		/*
		if( typeof(inpAutocomplete) === undefined ){
			// 
		}else{
		
			inpAutocomplete.deactivate();
		
		} */
}


